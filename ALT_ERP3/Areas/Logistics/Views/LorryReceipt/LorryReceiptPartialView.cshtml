                        $("#Direct").prop("checked", true)
                        $("#Godown1").prop("checked", false)
                        $("#Crossing").prop("checked", false)
                    }
                    else {
                        $("#Direct").prop("checked", false)
                        $("#Godown1").prop("checked", false)
                        $("#Crossing").prop("checked", true)
                    }
                    if (result.Deli == "G") {
                        $("#Door").prop("checked", false)
