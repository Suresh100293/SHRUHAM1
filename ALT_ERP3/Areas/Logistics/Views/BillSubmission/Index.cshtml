    }

    .ui-jqdialog {
        display: none;
        width: 300px;
        position: absolute;
        padding: .2em;
        font-size: 11px;
        overflow: visible;
        left: 30% !important;
        top: 35% !important;
        background-color: whitesmoke;
    }

    .ui-jqdialog-titlebar {
        background-color: gray;
    }

    button.active {
        background: linear-gradient(white,#BBBABA);
        color: black;
        font-weight: bold !important;
        border-top: solid;
        color: red !important;
        border-top-color: orangered;
