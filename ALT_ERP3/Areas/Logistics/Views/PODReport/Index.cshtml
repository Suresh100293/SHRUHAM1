        return false;
    };

    function GridReload() {

        $('#acledger').jqGrid('clearGridData');
        $("#acledger").jqGrid("GridUnload");
        $('#acledger').trigger('reloadGrid');

        $('.spinner').show();
        var ColName, Coldata;
        var mDate = ($('#Fromdate').val() + ':' + $('#ToDate').val());
        $("#hdnformatname").val($("#viewcode").val());

        var PODReceivedBranch_Direct = $('#PODReceivedBranch_Direct').val();
        var PODSendCustomer_Branch_File = $('#PODSendCustomer_Branch_File').val();
        var PODStatusActive_Closed_All = $('#PODStatusActive_Closed_All').val();


        var sPathUrl = '@Url.Action("GetGridStructureRecords")?Date=' + mDate + '&ViewDataId=@Model.ViewDataId' ;
        var sUrlpath = '@Url.Action("GetGridData")?Fromdate=' + $("#Fromdate").val() + '&ViewDataId=@Model.ViewDataId' + '&ReportTypeL=' + $("#ReportTypeL").val()+ '&ToDate=' + $("#ToDate").val() + '&PODReceivedBranch_Direct=' + PODReceivedBranch_Direct + '&PODSendCustomer_Branch_File=' + PODSendCustomer_Branch_File + '&PODStatusActive_Closed_All=' + PODStatusActive_Closed_All + '&EntryBranch=' + $("#EntryBranch").val() + '&FromBranch=' + $("#FromBranch").val() + '&ToBranch=' + $("#ToBranch").val() + '&Customer=' + $("#Customer").val();
        $.ajax({
            url: sPathUrl,
            type: "POST",
            contentType: "application/json; charset=utf-8",
            dataType: "json",
            success: function (data) {
                $('.spinner').show();
                var obj = $.parseJSON(data);
                ColName = obj[0].split(",");
                Coldata = obj[1];
                $('#openb').val(obj[2]);
                $('#closeb').val(obj[3]);
                $('#tcredit').val(obj[4]);
                $('#tdebit').val(obj[5]);
                CreateGridData();
                $('.spinner').hide();
            },
            error: function () {
                alert("Error with AJAX callback");
            }
        });

        function CreateGridData() {
            $('#acledger').jqGrid({
                url: sUrlpath,
                datatype: 'json',
                contentType: "application/json; charset=utf-8",
                cellEdit: true,
                mtype: 'POST',
                sortable: true,
                colNames: ColName,
                colModel: Coldata,
                pager: $('#pager1'),
                rowNum: 5000,
                rowList: [10, 15, 20, 50, 100, 500, 1000, 5000, 10000],
                height: $(window).height() - 265,
                frozen: false,
                sortorder: 'asc',
                gridview: true,
                viewrecords: true,
                scroll: false,
                autoencode: true,
                search: true,
                recordtext: "View {0} - {1} of <span style='font-weight:bold;font-size: 13px;'>{2}</span>",
                footerrow: true,
                userDataOnFooter: false,
                ondblClickRow: function (id) {
                    event.preventDefault();
                },
                beforeRequest: function () {
                    responsive_jqgridData($(".jqGrid"));
                },
                loadComplete: function () {
                    debugger;
                    jQuery("#acledger").jqGrid('footerData', 'set', {
                        'LrNo': 'Total: ' + $("#acledger").jqGrid('getCol', 'LrNo', false, 'count'),
                        'Qty':  $("#acledger").jqGrid('getCol', 'Qty', false, 'sum'),
                        'ActWt':  $("#acledger").jqGrid('getCol', 'ActWt', false, 'sum'),
                        'ChgWt':  $("#acledger").jqGrid('getCol', 'ChgWt', false, 'sum'),
                        'Amt':  $("#acledger").jqGrid('getCol', 'Amt', false, 'sum'),

                        'P/L AC': $("#acledger").jqGrid('getCol', 'P/L AC', false, 'sum')

                    }, false);

                    $("#acledger").jqGrid('setLabel', 'LRno', '', { 'background': 'rgb(120 203 184)' });
                    $("#acledger").jqGrid('setLabel', 'LRCreateDate', '', { 'background': 'rgb(120 203 184)' });
                    $("#acledger").jqGrid('setLabel', 'LRBookDate', '', { 'background': 'rgb(120 203 184)' });
                    $("#acledger").jqGrid('setLabel', '[From]', '', { 'background': 'rgb(120 203 184)' });
                    $("#acledger").jqGrid('setLabel', '[To]', '', { 'background': 'rgb(120 203 184)' });
                    $("#acledger").jqGrid('setLabel', 'Consignor', '', { 'background': 'rgb(120 203 184)' });
                    $("#acledger").jqGrid('setLabel', 'Consignee', '', { 'background': 'rgb(120 203 184)' });

                    $('.spinner').hide();
                    $('#pager1').height = 5;
                },
            });
            jQuery("#acledger").jqGrid('navGrid', "#pager1",
                { edit: false, add: false, del: false, search: true, refresh: true, searchtext: "filter"});
            jQuery("#acledger").jqGrid("setFrozenColumns");
        };

        function responsive_jqgridData(jqgrid) {
            jqgrid.find('.ui-jqgrid').addClass('clear-margin span12').css('width', '');
            jqgrid.find('.ui-jqgrid-view').addClass('clear-margin span12').css('width', '');
            jqgrid.find('.ui-jqgrid-view > div').eq(1).addClass('clear-margin span12').css('width', '').css('min-height', '0').css('display', 'grid');
            jqgrid.find('.ui-jqgrid-view > div').eq(2).addClass('clear-margin span12').css('width', '').css('min-height', '0').css('display', 'grid');
            jqgrid.find('.ui-jqgrid-sdiv').addClass('clear-margin span12').css('width', '');
            jqgrid.find('.ui-jqgrid-pager').addClass('clear-margin span12').css('width', '');
            jqgrid.find('#pager_right').css('width', '180px');
            jqgrid.find('.ui-state-default,.ui-widget-content .ui-state-default,.ui-widget-header .ui-state-default').css('background', '#e7e4e4');
            jqgrid.find('.ui-widget-content .ui-state-focus, .ui-widget-header .ui-state-focus .ui-state-hover').css('background', 'skyblue');
            jqgrid.find('.ui-jqgrid tr.jqgrow').css('font-family', 'Tahoma');
            jqgrid.find('.ui-jqgrid tr.jqgrow').css('font-size', '9pt');
        };
    }
    function ClickPara() {
        $('#Optionsmodal').modal('show');
    }
</script>

