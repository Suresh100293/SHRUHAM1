    function GetConsignmentDetails(value)
    {
        var TabID = "";
        var tabcontent = document.getElementsByClassName("tabcontent");
        for (i = 0; i < tabcontent.length; i++) {
            if (tabcontent[i].style.display != "none") {
                TabID = tabcontent[i].id.trim();
            }
        }
        $('#ConsignmentModels').modal('hide');
        $("#ConsignmentKey").val(value);

        //Generate Eway Bill
        if (TabID == "divLedgerTable")
        {
            var ModelData = {};
            ModelData["Document"] = value;
            var url = '@Url.Action("GetEwayBill")';
            var DTO = { Model: ModelData };
            $.ajax({
                type: "POST",
                dataType: "json",
                data: JSON.stringify(DTO),
                cache: false,
                url: url,
                contentType: "application/json; charset=utf-8",
                beforeSend: function () {
                    $('.spinner').show();
                },
                success: function (data) {
                    $("#ErrorMessage").hide();
                    $('.spinner').hide();
                    if (data.Status == "Success")
                    {

                        var LoadDownLoadFiles = $("#DivGenEway");
                        LoadDownLoadFiles.html('');
                        LoadDownLoadFiles.html(data.Html);
                        $("#btnGenEway").show();
                    }
                    else
                    {
                        alert(data.Errmsg);
                        var LoadDownLoadFiles = $("#DivGenEway");
                        LoadDownLoadFiles.html('');
