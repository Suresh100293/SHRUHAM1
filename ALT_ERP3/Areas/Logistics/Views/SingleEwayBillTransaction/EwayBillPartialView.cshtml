            $("#CurrentBranch,#AllBranch").prop("disabled", false);
        } else if ('@Model.tfatEwaySetup.ConsoleForUserBranch' == "True"&& '@Model.tfatEwaySetup.ConsoleForAllBranch' == "False") {

            $("#AllBranch").prop("checked", false);
            $("#AllBranch").prop("disabled", true);

