@model IEnumerable<ALT_ERP3.Areas.Logistics.Models.ArrivalDispatchVM>
@{
    Layout = null;
}
<style>
    .span6 {
        display: none !important;
    }
</style>
<script type="text/javascript">
    $(document).ready(function () {
        $('#tblUnLoadingList').DataTable();




        $(".btnFmUnLoadingDetails").click(function (event) {
             event.preventDefault();
            
            var url = '@Url.Action("GetUnLoadingDetails")';
            var DTO = { Lcno: this.id };
            $.ajax({
                type: "POST",
                dataType: "json",
                data: JSON.stringify(DTO),
                cache: false,
                url: url,
                contentType: "application/json; charset=utf-8",
                beforeSend: function () {
                    $('.spinner').show();
                },
                success: function (data) {
                    var LoadDownLoadFiles = $("#UnLoadingList");
                    $("#ErrorMessage").hide();
                    $('.spinner').hide();
                    $('#UnLoadingDetailsmodal').modal('show');
                    LoadDownLoadFiles.html('');
                    LoadDownLoadFiles.html(data.Html);
                },
            });
        });
    });
</script>
@if (Model != null)
{
    <div class="container-fluid">
        <center>
            <table class=" table-bordered table-bordered example table-responsive-md" id="tblUnLoadingList" style="width:-webkit-fill-available;">
                <thead id="Head">
                    <tr>
                        <th class="">FmNo</th>
                        <th class="">From</th>
                        <th class="">Route</th>
                        <th class="">To</th>
                        <th class="">VehicleNo</th>
                        <th class="">No-Of-LR</th>
                        <th class="">Total Weight</th>
                        <th class="">Total Qty</th>
                        <th class="">Un-Loading Details</th>
                        <th class=" hidden">UnloadLCNo</th>

                    </tr>
                </thead>
                <tbody id="LcCombo" style="max-height:300px;min-height:300px;width:100%;overflow:scroll;">
                    @foreach (var item in Model)
                    {
                        <tr class="">
                            <td class="">@item.Fmno</td>
                            <td class="">@item.From</td>
                            <td class="">@item.Route</td>
                            <td class="">@item.To</td>
                            <td class="">@item.VehicleNo</td>
                            <td class="">@item.NoofLr</td>
                            <td class="">@item.TotalWeight</td>
                            <td class="">@item.TotalQty</td>
                            <td class="">
                                <button class="btn btn-primary btnFmUnLoadingDetails" id="@item.LCno" style="font-size: 13px;padding:5px 12px;"><i class="glyphicon glyphicon-eye-open"></i></button>
                            </td>
                            <td class=" UnloadLcNo hidden">@item.LCno</td>
                        </tr>
                    }
                </tbody>

            </table>
        </center>
    </div>
    <br />
}




