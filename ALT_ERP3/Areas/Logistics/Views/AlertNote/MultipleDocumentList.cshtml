@model ALT_ERP3.Areas.Logistics.Models.AlertNoteVM
<script type="text/javascript">
    $(document).ready(function ()
    {
        $('#MultDocdumentTbl > tbody  > tr').dblclick(function () {
            var Row = $(this).closest('tr');

            $("#DocumentKey").val(Row.find('td:eq(0)').text());
            var url = '@Url.Action("CheckDocument")';
            var DTO = { Type: $("#AType").val(), DocNo: $("#TypeCode").val(), DocumentKey: $("#DocumentKey").val()};
            $.ajax({
                type: "POST",
                dataType: "json",
                data: JSON.stringify(DTO),
                cache: false,
                url: url,
                contentType: "application/json; charset=utf-8",
                beforeSend: function ()
                {
                    $('.spinner').show();
                },
                success: function (data)
                {
                    $("#ErrorMessage").hide();
                    $('.spinner').hide();
                    debugger;
                    var LoadDownLoadFiles = $("#BasicDetailsDiv");
                    if (data.Status == "Success")
                    {
                        LoadDownLoadFiles.html('');
                        LoadDownLoadFiles.html(data.BasicHtml);
                    }
                    else
                    {
                        var result = confirm(data.Message +"Are you sure To Continue?");
                        if (result)
                        {

                        } else {
                            $("#TypeCode").val("");
                            LoadDownLoadFiles.html('');
                        }
                    }
                },
            });

        });
    });
</script>
<div class="modal-dialog" style="width:unset;">
    <div class="modal-content">
        <div class="modal-header" style="background-color:#cccccc">
            <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
            <h4 class="modal-title">Multi Document</h4>
        </div>
        <div class="modal-body" style="padding:unset;min-height:450px;max-height:450px;overflow:auto;">
            <div class="panel-body">
                <table id="MultDocdumentTbl" class="zui-tableww" style="width:100%;">
                    <thead>
                        @if (Model.AType == "LR000" && Model.LRMasterslist != null)
                        {
                            <tr>
                                <th class="hidden">Tablekey</th>
                                <th>Date</th>
                                <th>LR No</th>
                                <th>From</th>
                                <th>To</th>
                                <th>Consignor</th>
                                <th>Consignee</th>
                                <th>Qty</th>
                                <th>Weight</th>
                                <th>EnteredBy</th>

                            </tr>
                        }
                        else if (Model.AType == "LC000" && Model.LCMasterslist != null)
                        {
                            <tr>
                                <th class="hidden">Tablekey</th>
                                <th>Date</th>
                                <th>Branch</th>
                                <th>LC NO</th>
                                <th>From</th>
                                <th>To</th>
                            </tr>
                        }
                        else if (Model.AType == "FM000" && Model.FMMasterslist != null)
                        {
                            <tr>
                                <th class="hidden">Tablekey</th>
                                <th>FM NO</th>
                                <th>Date</th>
                                <th>From</th>
                                <th>Via</th>
                                <th>To</th>
                                <th>Broker</th>
                                <th>Amt</th>
                            </tr>
                        }
                        else if (Model.AType == "FMH00" && Model.FMMasterslist != null)
                        {
                            <tr>
                                <th class="hidden">Tablekey</th>
                                <th>FM NO</th>
                                <th>Date</th>
                                <th>From</th>
                                <th>Via</th>
                                <th>To</th>
                                <th>Broker</th>
                                <th>Amt</th>
                            </tr>
                        }
                        else if ((Model.AType == "SLR00" || Model.AType == "SLW00") && Model.Saleslist != null)
                        {
                            <tr>
                                <th class="hidden">Tablekey</th>
                                <th>Date</th>
                                <th>Branch</th>
                                <th>Customer</th>
                                <th>Amt</th>
                                <th>ENTEREDBY</th>
                            </tr>
                        }
                        else if (Model.AType == "CMM00" && Model.Saleslist != null)
                        {
                            <tr>
                                <th class="hidden">Tablekey</th>
                                <th>Date</th>
                                <th>Branch</th>
                                <th>Bank</th>
                                <th>Amt</th>
                                <th>ENTEREDBY</th>
                            </tr>
                        }
                        else if (Model.AType == "PUR00" && Model.Purchaseslist != null)
                        {
                            <tr>
                                <th class="hidden">Tablekey</th>
                                <th>Date</th>
                                <th>Branch</th>
                                <th>Broker</th>
                                <th>Amt</th>
                                <th>ENTEREDBY</th>
                            </tr>
                        }

                    </thead>
                    <tbody style="background-color: burlywood !important;">

                        @if (Model.AType == "LR000" && Model.LRMasterslist != null)
                        {
                            foreach (var item in Model.LRMasterslist)
                            {
                                <tr>
                                    <td class="hidden">@item.TableKey</td>
                                    <td>@item.BookDate.ToShortDateString()</td>
                                    <td>@item.LrNo</td>
                                    <td>@item.Source</td>
                                    <td>@item.Dest</td>
                                    <td>@item.RecCode</td>
                                    <td>@item.SendCode</td>
                                    <td>@item.TotQty</td>
                                    <td>@item.ActWt</td>
                                    <td>@item.ENTEREDBY</td>

                                </tr>
                            }

                        }
                        else if (Model.AType == "FM000" && Model.FMMasterslist != null)
                        {
                            foreach (var item in Model.FMMasterslist)
                            {
                                <tr>
                                    <td class="hidden">@item.TableKey</td>
                                    <td>@item.FmNo</td>
                                    <td>@item.Date.ToShortDateString()</td>
                                    <td>@item.FromBranch</td>
                                    <td>@item.RouteViaName</td>
                                    <td>@item.ToBranch</td>
                                    <td>@item.BroCode</td>
                                    <td>@item.ChgAmt1</td>
                                </tr>
                            }

                        }
                        else if (Model.AType == "FMH00" && Model.FMMasterslist != null)
                        {
                            foreach (var item in Model.FMMasterslist)
                            {
                                <tr>
                                    <td class="hidden">@item.TableKey</td>
                                    <td>@item.FmNo</td>
                                    <td>@item.Date.ToShortDateString()</td>
                                    <td>@item.FromBranch</td>
                                    <td>@item.RouteViaName</td>
                                    <td>@item.ToBranch</td>
                                    <td>@item.BroCode</td>
                                    <td>@item.ChgAmt1</td>
                                </tr>
                            }

                        }
                        else if (Model.AType == "LC000" && Model.LCMasterslist != null)
                        {
                            foreach (var item in Model.LCMasterslist)
                            {
                                <tr>
                                    <td class="hidden">@item.TableKey</td>
                                    <td>@item.Date.ToShortDateString()</td>
                                    <td>@item.Branch</td>
                                    <td>@item.LCno.ToString()</td>
                                    <td>@item.FromBranch</td>
                                    <td>@item.ToBranch</td>
                                </tr>
                            }

                        }
                        else if ((Model.AType == "SLR00" || Model.AType == "SLW00") && Model.Saleslist != null)
                        {
                            foreach (var item in Model.Saleslist)
                            {
                                <tr>
                                    <td class="hidden">@item.TableKey</td>
                                    <td>@item.BillDate.Value.ToShortDateString()</td>
                                    <td>@item.Branch</td>
                                    <td>@item.Code</td>
                                    <td>@item.Amt</td>
                                    <td>@item.ENTEREDBY</td>
                                </tr>
                            }

                        }
                        else if (Model.AType == "CMM00" && Model.Saleslist != null)
                        {
                            foreach (var item in Model.Saleslist)
                            {
                                <tr>
                                    <td class="hidden">@item.TableKey</td>
                                    <td>@item.BillDate.Value.ToShortDateString()</td>
                                    <td>@item.Branch</td>
                                    <td>@item.Code</td>
                                    <td>@item.Amt</td>
                                    <td>@item.ENTEREDBY</td>
                                </tr>
                            }

                        }
                        else if (Model.AType == "PUR00" && Model.Purchaseslist != null)
                        {
                            foreach (var item in Model.Purchaseslist)
                            {
                                <tr>
                                    <td class="hidden">@item.TableKey</td>
                                    <td>@item.BillDate.Value.ToShortDateString()</td>
                                    <td>@item.Branch</td>
                                    <td>@item.Code</td>
                                    <td>@item.Amt</td>
                                    <td>@item.ENTEREDBY</td>
                                </tr>
                            }
                        }
                    </tbody>
                </table>
            </div>
        </div>
        <div class="modal-footer" style="background-color:#cccccc">
            <input type="submit" value="Close" class="btn btn-danger" data-dismiss="modal" />
        </div>
    </div>
</div>




