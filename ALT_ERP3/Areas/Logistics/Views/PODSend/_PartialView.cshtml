    function GridReloadSendBranch()
    {
        $('#example1').jqGrid('clearGridData');
        $("#example1").jqGrid("GridUnload");
        $('#example1').trigger('reloadGrid');

        $('#GetLRLIst').modal('show');

        $("#Search").val("");
        $("#SelectLR").val("");

        var ColName, Coldata;
        var lPath = window.location.pathname;
        var arrPath = lPath.split("/");
        var code = $("#btncode").val();
        var sPathUrl = '@Url.Action("GetGridStructureRecords")?id=PendingPODSendTOBranch';
