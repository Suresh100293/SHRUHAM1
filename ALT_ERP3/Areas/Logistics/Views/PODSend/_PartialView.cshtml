        var ModelData = {};
        ModelData["ViewDataId"] = "CurrentBranchStock";
        //ModelData["searchString"] = $('#txtDataSearch1').val();
        $('#txtDataSearch1').val("");
        var DTO = { Model: ModelData };
        $.ajax({
            url: sPathUrl,
            type: "POST",
            contentType: "application/json; charset=utf-8",
