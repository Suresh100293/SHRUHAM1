        $('#SaveDraftName').click(function (event) {
            if ($("#Draft_Name").val() == '' || $("#Draft_Name").val() == null) {
                $("#ErrorMessage").show();
                $("#ErrorMessage").text('Draft_Name Required.');
                return;
            }
            else
            {
                var url = '@Url.Action("CheckExistLR")';
                $.ajax({
                type: "POST",
                traditional: true,
                url: url,
                    data: { TableName: "LRMasterDraft", Colfield: "[DraftName]", Value: $('#Draft_Name').val(), SkipColumnName: "[LRno]", PKValue: '@Model.Document' },
