        $.ajax({
            type: "POST",
            dataType: "json",
            data: JSON.stringify(DTO),
            cache: false,
            url: url,
            contentType: "application/json; charset=utf-8",
            beforeSend: function ()
            {
                $('.spinner').show();
            },
            success: function (data)
            {
                if (data.Status == 'Success')
                {
                    $('.spinner').hide();
                    $('#BillParty').select2('data', { id: data.id, text: data.name });
                    $("#ErrorMessage").hide();
                } else {
                    $('.spinner').hide();
                }

            },
        });
    }
