<script type="text/javascript">
    $(document).ready(function () {
        GetMyFavorites('@Model.ViewDataId');
        $('#Fromdate').val('@Model.StartDate');
        $('#ToDate').val('@Model.EndDate');
        $('#Fromdate,#ToDate').datepicker({
            format: "dd/mm/yyyy",
            autoclose: true
        });

        $("#DeliveryDays").constrain({ allow: { regex: "[0-9]" } });

        $('#GetLRLIst').modal({
            backdrop: 'static',
            keyboard: false
        });
        $("#GetLRLIst").modal("hide");

        // Select All Check Then Calculate No of Consignment And Stock
        $("#DeselectAll").click(function () {
            var TotalQty = 0;
            var CheckBox = $("#DeselectAll").prop("checked");
            if (CheckBox)
            {
                $("#list > tbody  > tr").each(function () {
                    var Row = $(this).closest('tr');
                    debugger;
                    Row.find("input[class='DebitGrid']").prop("checked", true);
                    Row.find(".DELNarr").prop("disabled", false);
                    Row.find(".DeliveryDate").prop("disabled", false);
                    if (Row.find('td:eq(15)').text()!="") {
                        TotalQty += parseInt(Row.find('td:eq(15)').text());
                    }
                });
            }
            else
            {
                $("#list > tbody  > tr").each(function () {
                    var Row = $(this).closest('tr');
                    Row.find("input[class='DebitGrid']").prop("checked", false);
                    Row.find(".DELNarr").prop("disabled", true);
                    Row.find(".DeliveryDate").prop("disabled", true);
                });
            }
            debugger;
            $("#mTotalQty1").html(TotalQty);
            $("#mTotalLR1").html($('#list').find("input[class='DebitGrid']:checked").length);
        });
        // Select Manual Consignment then Calculate Qty all columns
        $("#SelectLR").blur(function () {

            var TotalQty = 0;
            var WantToselectLRNO = $("#SelectLR").val();
            var ScrollRow;
            var count = 0;
            debugger;
            if ($("#SelectLR").val() != "" && $("#SelectLR").val() != null) {
                $('#list tbody tr').each(function () {
                    debugger;
                    var Qty = 0
                    var Row = $(this).closest('tr');
                    var LRNO = Row.find('td:eq(10)').text();
                    var CheckBox = $(this).find("input[class='DebitGrid']").is(':checked');
                    Row.find(".DELNarr").prop("disabled", true);
                    Row.find(".DeliveryDate").prop("disabled", true);

                    if (LRNO == WantToselectLRNO) {
                        ScrollRow = Row;
                        Row.find("input[class='DebitGrid']").prop("checked", true);
                        Row.find(".DELNarr").prop("disabled", false);
                        Row.find(".DeliveryDate").prop("disabled", false);
                        Qty = parseInt(Row.find('td:eq(15)').text());

                        var objTable = $("#list");
                        var iRow = ScrollRow.find("td:eq(7)").text();
                        objTable.jqGrid('setSelection', iRow);
                        count++;
                    }
                    else {
                        if (CheckBox) {
                            Qty = parseInt(Row.find('td:eq(15)').text());
                            Row.find(".DELNarr").prop("disabled", false);
                            Row.find(".DeliveryDate").prop("disabled", false);
                        }
                    }
                    TotalQty += Qty;
                });

                var check = $('#list').find("input[class='DebitGrid']:checked").length;
                $("#mTotalQty1").html(TotalQty);
                $("#mTotalLR1").html(check);
                debugger;
                if (count == 0) {
                    alert("Not Found");
                    $("#Search").focus();
                }
                else {
                    $("#SelectLR").focus();
                }
            }
            $("#SelectLR").val("");
        });
        // Search all columns
        $('#Search').keyup(function () {
            var value = $(this).val();
            if (value != "") {
                $('#list > tbody > tr').removeClass('SearchRow');

                $("#list > tbody > tr > td ").each(function () {
                    if ($(this).text().toLowerCase().indexOf(value.toLowerCase()) >= 0) {
                        var dd = $(this).text().toLowerCase();
                        $(this).addClass("SearchRow");
                    }
                    else {
                        $(this).removeClass("SearchRow");
                    }
                });
            }
            else {
                $('#list td').removeClass('SearchRow');
            }
        });


        $('#GodownPickData').click(function (event) {
            event.preventDefault();
            $("#DeselectAll").prop('checked', false);
            $("#Search").val("");
            $("#SelectLR").val("");
            $('#list').jqGrid('clearGridData');
            $("#list").jqGrid("GridUnload");
            $('#list').trigger('reloadGrid');
            GridReload("1");
        });
        $('#TransitPickData').click(function (event) {
            event.preventDefault();
            $("#DeselectAll").prop('checked', false);
            $("#Search").val("");
            $("#SelectLR").val("");
            $('#list').jqGrid('clearGridData');
            $("#list").jqGrid("GridUnload");
            $('#list').trigger('reloadGrid');
            GridReload("2");
        });
        $('#AllPickData').click(function (event) {
            event.preventDefault();
            $("#DeselectAll").prop('checked', false);
            $("#Search").val("");
            $("#SelectLR").val("");
            $('#list').jqGrid('clearGridData');
            $("#list").jqGrid("GridUnload");
            $('#list').trigger('reloadGrid');
            GridReload("0");
        });

        $('#btnGetLc').click(function (event) {
            event.preventDefault();
            var url = '@Url.Action("GridView")';
            var myTableArray = [];
            $("#list > tbody  > tr").each(function () {
                if ($(this).find("input[class='DebitGrid']").prop("checked") == true) {
                    debugger;
                    var myDets =
                    {
                        StockTablekey: $(this).find('td:eq(4)').text(),
                        Tablekey: $(this).find('td:eq(5)').text(),
                        DeliveryDate: $(this).find('.DeliveryDate').val(),
                        DeliveryRemark: $(this).find('.DELNarr').val(),
                        Lrno: $(this).find('td:eq(10)').text(),
                        BookDate: $(this).find('td:eq(11)').text(),
                        StockBranch: $(this).find('td:eq(12)').text(),
                        StockType: $(this).find('td:eq(13)').text(),
                        TotQty: $(this).find('td:eq(14)').text(),
                        StockQty: $(this).find('td:eq(15)').text(),
                        ChgWt: $(this).find('td:eq(16)').text(),
                        ActWt: $(this).find('td:eq(17)').text(),
                        ChgTypeName: $(this).find('td:eq(18)').text(),
                        UnitName: $(this).find('td:eq(19)').text(),
                        FromName: $(this).find('td:eq(20)').text(),
                        ToName: $(this).find('td:eq(21)').text(),
                        ConsignerName: $(this).find('td:eq(22)').text(),
                        ConsigneeName: $(this).find('td:eq(23)').text(),
                        LRtypeName: $(this).find('td:eq(24)').text(),
                        Delivery: $(this).find('td:eq(25)').text(),
                        Colln: $(this).find('td:eq(26)').text(),
                    }
                    myTableArray.push(myDets);
                }
            });
            var check = $('#list').find("input[class='DebitGrid']:checked").length;
            if (check > 0)
            {
                $("#GetLRLIst").modal("hide");
                var ModelData = {};
                ModelData["GridDetails"] = myTableArray;
                var DTO = { mModel:ModelData };
                $.ajax({
                    type: "POST",
                    dataType: "json",
                    data: JSON.stringify(DTO),
                    cache: false,
                    url: url,
                    contentType: "application/json; charset=utf-8",
                    beforeSend: function ()
                    {
                        $('.spinner').show();
                    },
                    success: function (data)
                    {
                        var LoadDownLoadFiles1 = $("#SelectedLrShow");
                        $("#ErrorMessage").hide();
                        $('.spinner').hide();
                        LoadDownLoadFiles1.html('');
                        LoadDownLoadFiles1.html(data.Html);
                        $("#txtSearchLr").val("");
                    },
                });
            }
            else
            {
                alert("Please Select Stock Required...");
            }
        });

        $("#Submit").click(function (event) {
            if ($("#example tbody tr").length<=0) {
                alert('Atleast One Consignment Stock Required To Delivery...');
                return;
            }
            $('#Submit').prop("disabled", true);
            var url = '@Url.Action("SaveData")';
            var ModelData = {};
            ModelData["Mode"] = '@Model.Mode';
            ModelData["Header"] = '@Model.Header';
            var DTO = { mModel: ModelData };
            $.ajax({
                type: "POST",
                dataType: "json",
                data: JSON.stringify(DTO),
                cache: false,
                url: url,
                contentType: "application/json; charset=utf-8",
                beforeSend: function () {
                    $('.spinner').show();
                },
                success: function (data) {
                    if (data.Status == 'Success') {
                        alert("Documents Save Sucessfully...!");
                        $('.spinner').hide();
                        window.location.href = "/Logistics/MasterGrid/Index?ViewDataId=@Model.ViewDataId&Module=@Model.Module&TableName=@Model.TableName&OptionType=@Model.OptionType&OptionCode=@Model.OptionCode&Header=@Model.Header&Controller2=@Model.Controller2&MainType=@Model.MainType";
                        $("#ErrorMessage").hide();
                    }
                    else {
                        $('.spinner').hide();
                        alert(data.Message);
                    }
                },
            });
        });

    });
    function GridReload(value) {
        var ColName, Coldata;
        var sPathUrl = '@Url.Action("GetGridStructureRecords")?id=PickStockMultiDelivery';
        var ExistLr = "";
        $("#example > tbody  > tr").each(function () {
            var tds = $(this).find('td');
            if (ExistLr == "") {
                ExistLr = ExistLr + tds.eq(0).text() + "',"
            }
            else {
                ExistLr = ExistLr + "'" + tds.eq(0).text() + "',"
            }
        });
        if (ExistLr != "") {
            ExistLr = ExistLr.substr(0, ExistLr.length - 2)
        }

        var sUrlpath = '@Url.Action("GetGridData")?ViewDataId=PickStockMultiDelivery&mVar4=' + ExistLr + '&FromDate=' + $("#Fromdate").val() + '&ToDate=' + $("#ToDate").val() + '&mVar5=' + $("#DeliveryDays").val();

        if (value == "1")
        {
            sUrlpath = '@Url.Action("GetGridData1")?ViewDataId=PickStockMultiDelivery&mVar4=' + ExistLr + '&FromDate=' + $("#Fromdate").val() + '&ToDate=' + $("#ToDate").val() + '&mVar5=' + $("#DeliveryDays").val();
        }
        else if (value == "2")
        {
            sUrlpath = '@Url.Action("GetGridData2")?ViewDataId=PickStockMultiDelivery&mVar4=' + ExistLr + '&FromDate=' + $("#Fromdate").val() + '&ToDate=' + $("#ToDate").val() + '&mVar5=' + $("#DeliveryDays").val();
        }

        $.ajax({
            url: sPathUrl,
            type: "POST",
            contentType: "application/json; charset=utf-8",
            dataType: "json",
            beforeSend: function () {
                $('.spinner').show();
            },
            success: function (data) {
                var obj = $.parseJSON(data);
                ColName = obj[0].split(",");
                Coldata = obj[1];
                CreateGridData();
                $('.spinner').hide();
            },
            error: function () {
                alert("Error with AJAX callback");
            }
        });
        function CreateGridData() {
            $('#list').jqGrid({
                url: sUrlpath,
                datatype: 'json',
                mtype: 'POST',
                sortable: true,
                colNames: ColName,
                colModel: Coldata,
                pager: $('#pager'),
                rowNum: @Session["GridRows"],
                rowList: [10, 15, 20, 50, 100, 500, 1000, 5000, 10000],
                height: 350,
                sortname: '',
                frozen: false,
                sortorder: 'asc',
                gridview: true,
                viewrecords: true,
                autowidth: false,
                shrinkToFit: false,
                search: true,
                scrollrows: true,
                onSelectRow: function (id, status, e) {
                    $('.spinner').hide();
                },
                ondblClickRow: function (id) {
                },

                beforeRequest: function () {
                    responsive_jqgridData($(".jqGrid"));
                },
                gridComplete: function () {
                    var ids = jQuery("#list").jqGrid('getDataIDs');
                    for (var i = 0; i < ids.length; i++) {
                        var rowId = ids[i];
                        var beChecked = "<input type ='checkbox' id='" + rowId + "' value='" + jQuery("#list").getCell(rowId, "Recordekey") + "'  onclick='JQGridCheckbox();' class='DebitGrid' name='DebitGrid'  style='height: 22px;width: 19px;' />";
                        //var Narr = "<textarea class='DELNarr' name='w3review' rows='2' cols='30' maxlength='150' disabled></textarea>";
                        var DeliveryDate = "<input value='" + jQuery("#list").getCell(rowId, "Delivery Date") + "' class='form-control DeliveryDate' name='DeliveryDate' type='text' disabled>";
                        jQuery("#list").jqGrid('setRowData', rowId, { 'Select': beChecked, 'Delivery Date': DeliveryDate });
                    }
                    $('.DeliveryDate').datepicker({
                        format: "dd/mm/yyyy",
                        autoclose: true
                    });
                },
            });
            jQuery("#list").jqGrid('navGrid', "#pager",
            { edit: false, add: false, del: false, print:false, search: true, refresh: false, searchtext: "filter" });
            jQuery("#list").jqGrid("setFrozenColumns");
        };
        function responsive_jqgridData(jqgrid) {
            jqgrid.find('.ui-jqgrid').addClass('clear-margin span12').css('width', '');
            jqgrid.find('.ui-jqgrid-view').addClass('clear-margin span12').css('width', '');
            jqgrid.find('.ui-jqgrid-view > div').eq(1).addClass('clear-margin span12').css('width', '').css('min-height', '0').css('display', 'grid');
            jqgrid.find('.ui-jqgrid-view > div').eq(2).addClass('clear-margin span12').css('width', '').css('min-height', '0').css('display', 'grid');
            jqgrid.find('.ui-jqgrid-sdiv').addClass('clear-margin span12').css('width', '');
            jqgrid.find('.ui-jqgrid-pager').addClass('clear-margin span12').css('width', '');
            jqgrid.find('#pager_right').css('width', '180px');
            jqgrid.find('.ui-state-default,.ui-widget-content .ui-state-default,.ui-widget-header .ui-state-default').css('background', '#e7e4e4');
            jqgrid.find('.ui-widget-content .ui-state-focus, .ui-widget-header .ui-state-focus .ui-state-hover').css('background', 'skyblue');
            $('#GetLRLIst').modal('show');
        };
    };
    function JQGridCheckbox() {
        var TotalQty = 0, count=0;

        $('#list tbody tr').each(function () {
            debugger;
            var Row = $(this).closest('tr');
            var CheckBox = $(this).find("input[class='DebitGrid']").is(':checked');
            Row.find(".DELNarr").prop("disabled", true);
            Row.find(".DeliveryDate").prop("disabled", true);
            var Qty = 0;
            if (CheckBox)
            {
                Qty = parseInt(Row.find('td:eq(15)').text());
                Row.find(".DELNarr").prop("disabled", false);
                Row.find(".DeliveryDate").prop("disabled", false);
                count++;
            }
            TotalQty += Qty;
        });


        $("#mTotalQty1").html(TotalQty);
        $("#mTotalLR1").html(count);
    }
</script>

