<section class="panel">
    <header class="panel-heading"></header>
    <div class="panel-body">
        <table class="table table-striped table-bordered">
            <thead>
                <tr role="row" style="font-family:Tahoma;font-size:9pt;height:25px;">
                    <th style="vertical-align:inherit;">Type</th>
                    <th style="vertical-align:inherit;">Prefix</th>
                    <th style="vertical-align:inherit;">Serial</th>
                    <th style="vertical-align:inherit;">Bill No.</th>
                    <th style="vertical-align:inherit;">Doc.Date</th>
                </tr>
            </thead>
            <tbody>

                @if (Model.RecDList1 != null)
                {
                    foreach (var item in Model.RecDList1)
                    {
                        <tr role="row" style="font-family:Tahoma;font-size:9pt;height:25px;">
                            <td style="vertical-align:inherit;">@item.Type</td>
                            <td style="vertical-align:inherit;">@item.Prefix</td>
                            <td style="vertical-align:inherit;">@item.Srl</td>
                            <td style="vertical-align:inherit;text-align:right;">@item.Amount</td>
                            <td style="vertical-align:inherit;">@item.Docdate</td>
                        </tr>
                    }
                }
                
            </tbody>
        </table>
    </div>
</section>
