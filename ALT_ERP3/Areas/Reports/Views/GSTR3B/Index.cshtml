        <script>
            function openTab(evt, tabName) {
                var i, tabcontent, tablinks;
                tabcontent = document.getElementsByClassName("tabcontent");
                for (i = 0; i < tabcontent.length; i++) {
                    tabcontent[i].style.display = "none";
                }
                tablinks = document.getElementsByClassName("tablinks");
                for (i = 0; i < tablinks.length; i++) {
                    tablinks[i].className = tablinks[i].className.replace(" active", "");
                }
                document.getElementById(tabName).style.display = "block";
                evt.currentTarget.className += " active";
                if (tabName == "taboutward") {
                    $('#outward').jqGrid('clearGridData');
                    $("#outward").jqGrid("GridUnload");
                    $('#outward').trigger('reloadGrid');
                    gridoutward();
                } else if (tabName == "taburd") {
                    $('#urd').jqGrid('clearGridData');
                    $("#urd").jqGrid("GridUnload");
                    $('#urd').trigger('reloadGrid');
                    gridurd();
                } else if (tabName == "tabitc") {
                    $('#itc').jqGrid('clearGridData');
                    $("#itc").jqGrid("GridUnload");
                    $('#itc').trigger('reloadGrid');
                    griditc();
                } else if (tabName == "tabexempted") {
                    $('#exempted').jqGrid('clearGridData');
                    $("#exempted").jqGrid("GridUnload");
                    $('#exempted').trigger('reloadGrid');
                    gridexempted();
                //} else if (tabName == "tabpayment") {
                //    $('#payment').jqGrid('clearGridData');
                //    $("#payment").jqGrid("GridUnload");
                //    $('#payment').trigger('reloadGrid');
                //    gridpayment();
                }
            }
            document.getElementById("options").click();
        </script>
