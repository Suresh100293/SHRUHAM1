    @*chkReturnP = Consider Purchase Return in CDNR
    chkReturnPR = Don't Consider Purchase Return in HSN Summary
    chkShowPurRet = Consider Purchase Return in B2B
    chkReturnSR = Don't Consider Sales Return in GSTR1
    chkDontSRinCS = Don't Consider Sales Return in B2CS
    chkPRRetDoc = Don't Consider Purchase Return in Docs*@
    function gridb2b() {
        event.preventDefault();
        var ColName, Coldata;
        var mDate = ($('#Fromdate').val() + ':' + $('#ToDate').val());
        $("#hdnformatname").val("GSTR1-b2b");
        GetCheckedString();
        var sPathUrl = '@Url.Action("GetGridStructureRecords")?Date=' + mDate + '&ViewDataId=' + $("#hdnformatname").val() + '&SelectContent=' + mchk;
        var sUrlpath = '@Url.Action("GetGridData")?Date=' + mDate + '&ViewDataId=' + $("#hdnformatname").val() + '&SelectContent=' + mchk;
        $.ajax({
            url: sPathUrl,
            type: "POST",
            contentType: "application/json; charset=utf-8",
            dataType: "json",
            success: function (data) {
                $('.spinner').show();
                var obj = $.parseJSON(data);
                ColName = obj[0].split(",");
                Coldata = obj[1];
                creategridb2b();
                $('.spinner').hide();
            },
            error: function () {
                alert("Error with AJAX callback");
            }
        });

        function creategridb2b() {
            $('#b2b').jqGrid({
                url: sUrlpath,
                datatype: 'json',
                contentType: "application/json; charset=utf-8",
                cellEdit: false,
                mtype: 'POST',
                sortable: true,
                colNames: ColName,
                colModel: Coldata,
                pager: $('#pagerb2b'),
                rowNum: 5000,
                rowList: [10, 15, 20, 50, 100, 500, 1000, 5000, 10000],
                height: $(window).height() - 300,
                frozen: false,
                sortorder: 'asc',
                gridview: true,
                viewrecords: true,
                scroll: false,
                autoencode: true,
                search: true,
                footerrow: true,
                userDataOnFooter: true,
                recordtext: "View {0} - {1} of <span style='font-weight:bold;font-size: 13px;'>{2}</span>",
                beforeRequest: function () {
                    responsive_jqgridData($(".jqGrid"));
                },
                onSelectRow: function (id, status, e) {
                    e.preventDefault();
                    mcurrentrow = id;
                    $('#b2bsubgrid').jqGrid('clearGridData');
                    $('#b2bsubgrid').jqGrid("GridUnload");
                    $('#b2bsubgrid').trigger('reloadGrid');
                    GridReload2($('#b2b').jqGrid('getCell', id, 'Document'));
                },
                loadComplete: function () {
                    $('.spinner').hide();
                    $('#pagerb2b').height = 5;
                },
                gridComplete: function () {
                    $('.spinner').hide();
                    var trxrows = $('#b2b').getDataIDs();
                    $('#b2b').jqGrid('setRowData', trxrows[trxrows.length - 1], false, { color: 'Black', weightfont: 'bold', background: '#E9E3F4' });
                },
            });
            jQuery("#b2b").jqGrid('navGrid', "#pagerb2b",
                { edit: false, add: false, del: false, search: true, refresh: true, searchtext: "filter" });
            jQuery("#b2b").jqGrid("setFrozenColumns");
        };

        function responsive_jqgridData(jqgrid) {
            jqgrid.find('.ui-jqgrid').addClass('clear-margin span12').css('width', '');
            jqgrid.find('.ui-jqgrid-view').addClass('clear-margin span12').css('width', '');
            jqgrid.find('.ui-jqgrid-view > div').eq(1).addClass('clear-margin span12').css('width', '').css('min-height', '0').css('display', 'grid');
            jqgrid.find('.ui-jqgrid-view > div').eq(2).addClass('clear-margin span12').css('width', '').css('min-height', '0').css('display', 'grid');
            jqgrid.find('.ui-jqgrid-sdiv').addClass('clear-margin span12').css('width', '');
            jqgrid.find('.ui-jqgrid-pager').addClass('clear-margin span12').css('width', '');
            jqgrid.find('#pager_right').css('width', '180px');
            jqgrid.find('.ui-state-default,.ui-widget-content .ui-state-default,.ui-widget-header .ui-state-default').css('background', '#e7e4e4');
            jqgrid.find('.ui-widget-content .ui-state-focus, .ui-widget-header .ui-state-focus .ui-state-hover').css('background', 'skyblue');
            jqgrid.find('.ui-jqgrid tr.jqgrow').css('font-family', 'Tahoma');
            jqgrid.find('.ui-jqgrid tr.jqgrow').css('font-size', '9pt');
        };
    }

    function GridReload2(mDocument) {
        var ColName, Coldata;
        var mDate = ($('#Fromdate').val() + ':' + $('#ToDate').val());
        var sPathUrl = '@Url.Action("GetSubGridStructure")?ViewDataId=GSTR1-b2b';
        var sUrlpath = '@Url.Action("GetSubGridData")?Document=' + mDocument + '&Date=' + mDate + '&ViewDataId=GSTR1-b2b';
        var sidix = "";
        $.ajax({
            url: sPathUrl,
            type: "POST",
            contentType: "application/json; charset=utf-8",
            dataType: "json",
            success: function (data) {
                $('.spinner').show();
                var obj = $.parseJSON(data);
                ColName = obj[0].split(",");
                Coldata = obj[1];
                CreateGridData2();
                $('.spinner').hide();
            },
            error: function () {
                alert("Error Found..");
            }
        });

        function CreateGridData2() {
            $('#b2bsubgrid').jqGrid({
                url: sUrlpath,
                datatype: 'json',
                mtype: 'POST',
                sortable: true,
                colNames: ColName,
                colModel: Coldata,
                //pager: $('#pager2'),
                rowNum: 50000,
                rowList: [10, 15, 20, 50, 100, 500, 1000, 3000, 5000, 10000, 50000],
                height: $(window).height() - 300,
                sortname: sidix,
                frozen: false,
                sortorder: 'desc',
                gridview: true,
                viewrecords: true,
                scroll: false,
                autoencode: true,
                autowidth: false,
                shrinkToFit: false,
                search: true,
                beforeRequest: function () {
                    responsive_jqgridData($(".jqGrid"));
                }
            });
            jQuery("#b2bsubgrid").jqGrid("setFrozenColumns");
        };

        function responsive_jqgridData(jqgrid) {
            jqgrid.find('.ui-jqgrid').addClass('clear-margin span12').css('width', '');
            jqgrid.find('.ui-jqgrid-view').addClass('clear-margin span12').css('width', '');
            jqgrid.find('.ui-jqgrid-view > div').eq(1).addClass('clear-margin span12').css('width', '').css('min-height', '0').css('display', 'grid');
            jqgrid.find('.ui-jqgrid-view > div').eq(2).addClass('clear-margin span12').css('width', '').css('min-height', '0').css('display', 'grid');
            jqgrid.find('.ui-jqgrid-sdiv').addClass('clear-margin span12').css('width', '');
            jqgrid.find('.ui-state-default,.ui-widget-content .ui-state-default,.ui-widget-header .ui-state-default').css('background', '#e7e4e4');
            jqgrid.find('.ui-widget-content .ui-state-focus, .ui-widget-header .ui-state-focus .ui-state-hover').css('background', 'skyblue');
        };
    }

