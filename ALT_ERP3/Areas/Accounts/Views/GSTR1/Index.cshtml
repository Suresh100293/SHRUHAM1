        <script>
            function openTab(evt, tabName) {
                var i, tabcontent, tablinks;
                tabcontent = document.getElementsByClassName("tabcontent");
                for (i = 0; i < tabcontent.length; i++) {
                    tabcontent[i].style.display = "none";
                }
                tablinks = document.getElementsByClassName("tablinks");
                for (i = 0; i < tablinks.length; i++) {
                    tablinks[i].className = tablinks[i].className.replace(" active", "");
                }
                document.getElementById(tabName).style.display = "block";
                evt.currentTarget.className += " active";
                if (tabName == "tabb2b") {
                    $('#b2b').jqGrid('clearGridData');
                    $("#b2b").jqGrid("GridUnload");
                    $('#b2b').trigger('reloadGrid');
                    gridb2b();
                } else if (tabName == "tabb2cl") {
                    $('#b2cl').jqGrid('clearGridData');
                    $("#b2cl").jqGrid("GridUnload");
                    $('#b2cl').trigger('reloadGrid');
                    gridb2cl();
                } else if (tabName == "tabb2cs") {
                    $('#b2cs').jqGrid('clearGridData');
                    $("#b2cs").jqGrid("GridUnload");
                    $('#b2cs').trigger('reloadGrid');
                    gridb2cs();
                } else if (tabName == "tabcdnr") {
                    $('#cdnr').jqGrid('clearGridData');
                    $("#cdnr").jqGrid("GridUnload");
                    $('#cdnr').trigger('reloadGrid');
                    gridcdnr();
                } else if (tabName == "tabcdnur") {
                    $('#cdnur').jqGrid('clearGridData');
                    $("#cdnur").jqGrid("GridUnload");
                    $('#cdnur').trigger('reloadGrid');
                    gridcdnur();
                } else if (tabName == "tabexp") {
                    $('#exp').jqGrid('clearGridData');
                    $("#exp").jqGrid("GridUnload");
                    $('#exp').trigger('reloadGrid');
                    gridexp();
                } else if (tabName == "tabat") {
                    $('#at').jqGrid('clearGridData');
                    $("#at").jqGrid("GridUnload");
                    $('#at').trigger('reloadGrid');
                    gridat();
                } else if (tabName == "tabatadj") {
                    $('#atadj').jqGrid('clearGridData');
                    $("#atadj").jqGrid("GridUnload");
                    $('#atadj').trigger('reloadGrid');
                    gridatadj();
                } else if (tabName == "tabexemp") {
                    $('#exemp').jqGrid('clearGridData');
                    $("#exemp").jqGrid("GridUnload");
