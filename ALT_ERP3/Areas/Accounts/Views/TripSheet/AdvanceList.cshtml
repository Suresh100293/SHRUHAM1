
@model ALT_ERP3.Areas.Accounts.Models.TripSheetModel
@using Common

<table class="zui-table" style="border-color:gray;width:100%;">
    <thead>
        <tr role="row" style="font-family:Tahoma;font-size:9pt;">
            <th style="width: 1%;vertical-align:inherit;">
                Branch
            </th>
            <th style="width: 1%;vertical-align:inherit;">
                FM No
            </th>
            <th style="width: 1%;vertical-align:inherit;">
                Date
            </th>
            <th style="width: 1%;vertical-align:inherit;">
                Advance
            </th>

        </tr>
    </thead>
    <tbody>
        @if (Model.AdvanceList != null)
        {
            foreach (var item in @Model.AdvanceList.ToList())
            {
                <tr style="font-family:Tahoma;font-size:9pt;">
                    <td style="vertical-align:inherit;">
                        @item.Branch
                    </td>
                    <td style="vertical-align:inherit;">
                        @item.FMNo
                    </td>
                    <td style="vertical-align:inherit;">
                        @item.FMDate
                    </td>
                    <td style="vertical-align:inherit;">
                        @item.Amount
                    </td>
                </tr>
            }
        }

    </tbody>
</table>