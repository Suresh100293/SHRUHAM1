        function GetBroadCast()
        {
            var ModelData = {};
            var url='/Logistics/BroadcastMaster/GetBroadCast';
            var DTO = { broadCast: ModelData };
            $.ajax({
                type: "POST",
                dataType: "json",
                data: JSON.stringify(DTO),
                cache: false,
                url: url,
                contentType: "application/json; charset=utf-8",
                beforeSend: function () {
                    $('.spinner').hide();
                },
                success: function (data)
                {
                    if (data.Status == 'Success')
                    {
                        $('.spinner').hide();
                        if (data.Blink == true && data.Scroll == true)
                        {
                            $("#BradCastDiv").addClass("Blink");
                            $("#BradCastDiv").addClass("Scroll");
                            $("#BradCastText").text(data.Narr);
                            $('#BradCastText').css("font-family", data.FontFamily);
                            if (data.Bold == true)
                            {
                                $("#BradCastText").addClass("bold");
                            }
                            else
                            {
                                $("#BradCastText").removeClass("bold");
                            }
                            $("#BradCastText").css({ color: data.Color });
                        }
                        else if (data.Blink == true)
                        {
                            $("#BradCastDiv").addClass("Blink");
                            $("#BradCastText").text(data.Narr);
                            $('#BradCastText').css("font-family", data.FontFamily);
                            $('#BradCastText').css("text-align", "center");
                            if (data.Bold == true) {
                                $("#BradCastText").addClass("bold");
                            }
                            else {
                                $("#BradCastText").removeClass("bold");
                            }
                            $("#BradCastText").css({ color: data.Color });
                        }
                        else if (data.Scroll == true)
                        {
                            $("#BradCastDiv").addClass("Scroll");
                            $("#BradCastText").text(data.Narr);
                            $('#BradCastText').css("font-family", data.FontFamily);
                            if (data.Bold == true) {
                                $("#BradCastText").addClass("bold");
                            }
                            else {
                                $("#BradCastText").removeClass("bold");
                            }
                            $("#BradCastText").css({ color: data.Color });
                        }
                        else
                        {
                            $("#BradCastText").text(data.Narr);
                            $('#BradCastText').css("font-family", data.FontFamily);
                            $('#BradCastText').css("text-align", "center");
                            $('#BradCastText').css("font-size", "9pt");
                            if (data.Bold == true) {
                                $("#BradCastText").addClass("bold");
                            }
                            else {
                                $("#BradCastText").removeClass("bold");
                            }
                            $("#BradCastText").css({ color: data.Color });
                        }
                    }
                    else {
                        $('.spinner').hide();
                    }
                },
            });
        }
