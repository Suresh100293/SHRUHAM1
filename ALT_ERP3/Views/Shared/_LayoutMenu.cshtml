        function RunMenuOption()
        {
            $('.spinner').show();
            var mval = $(txtmenusearch).select2('data').text;
            $.ajax({
                url: '@Url.Action("GetMenuURL")?moptioncode=' + $('#txtmenusearch').val() + '&moptionname=' + mval,
                data: [],
                success: function (result) {
                    $('.spinner').hide();
                    if (result.url != "") {
                        $('#txtmenusearch').val("");
                        window.open(result.url, "_self");
                    } else {
                        alert(result.Message);
                        $('#txtmenusearch').val("");
                    }
                }
            });
        };
        function MenuSearch()
        {

            var url = '@Url.Action("SearchMenu")';
            $('#txtmenusearch').select2({
                minimumInputLength: 3,
                placeholder: 'Search',
                ajax: {
                    url: url,
                    dataType: 'json',
                    enableFiltering: true,
                    allowClear: true,
                    minimumInputLength: 3,
                    multiple: true,
                    data: function (term, page) {
                        return {
                            types: ["exercise"],
                            limit: -1,
                            term: term
                        };
                    },
                    width: 300,
                    results: function (data, page) {
                        clientObj = JSON.stringify(data);
                        return {
                            results: $.map(data, function (item) {
                                return {
                                    text: item.Name,
                                    id: item.Code
                                }
                            })
                        };
                    }
                },
                formatResult: function (exercise) {
                    return "<div class='row-fluid'><div class=''><span class='span1'><div style='color:" + exercise.alias + "'>" + exercise.text +
                        "</div></span></div></div>";
                },
                formatSelection: function (exercise) {
                    return "<div class='row-fluid'><div class=''><span class='span1'><div style='color:" + exercise.alias + "'>" + exercise.text +
                        "</div></span></div></div>";
                },
            });
        };
