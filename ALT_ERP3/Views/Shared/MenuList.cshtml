@if (ViewBag.Logistics != null)
{
    <div class="col-md-2" style="padding-left:5px;padding-left:15px;">
        <div class="panel" style="background-color:rgb(215, 215, 214);padding-top:5px;padding-bottom:2px;padding-left:12px;border-radius:0px;border-bottom:solid 5px;border-bottom-color:#3399ff;margin-bottom:6px;height:39px;">
            <label style="color:black;font-size:18px;font-weight:900;">Logistics</label>
        </div>
        <br />
        <div id="menu">
            <ul class="list-inline-item" style="font-family:Verdana; margin-top:-1px;height:auto;width:100%;background-color:whitesmoke;">
                @foreach (var item in (ViewBag.Logistics as IEnumerable<ALT_ERP3.Areas.DashBoard.Models.TFATMenuVM>).ToList())
                {
                    if (item.ZoomURL == null && item.Hide == false)
                    {
                        //<li style="padding-bottom:5px;"><a href="@Url.Action("Index", "" + item.Controller + "", new { Area = "" + item.ParentMenu + "", ViewDataId = "" + item.FormatCode + "", MainType = "" + item.MainType + "", SubType = "" + item.SubType + "", Module = "" + item.ModuleName + "", TableName = "" + item.TableName + "", optiontype = "" + item.OptionType.Trim() + "", optioncode = "" + item.OptionCode.Trim() + "", Header = "" + item.Menu.Trim() + "", Controller2 = "" + item.Controller2.Trim() + "" })">@item.Menu</a></li>
                        <a href="@Url.Action("Index", "" + item.Controller + "", new { Area = "" + item.ParentMenu + "", ViewDataId = "" + item.FormatCode + "", MainType = "" + item.MainType + "", SubType = "" + item.SubType + "", Module = "" + item.ModuleName + "", TableName = "" + item.TableName + "", optiontype = "" + item.OptionType.Trim() + "", optioncode = "" + item.OptionCode.Trim() + "", Header = "" + item.Menu.Trim() + "", Controller2 = "" + item.Controller2.Trim() + "" })"><li style="padding-bottom:5px;">@item.Menu</li></a>
                    }
                    else
                    {
                        if (item.ZoomURL == "M" && item.Hide == false)
                        {
                            <li style="padding-bottom:5px;">
                                <a href="#">@item.Menu <span class="glyphicon glyphicon-chevron-right"></span></a>
                                <ul>
                                    @foreach (var item1 in (ViewBag.Logistics as IEnumerable<ALT_ERP3.Areas.DashBoard.Models.TFATMenuVM>).ToList())
                                    {
                                        if (item.Level.ToString() == item1.ZoomURL && item1.Hide == false)
                                        {
                                            <a href="@Url.Action("Index", "" + item1.Controller + "", new { Area = "" + item.ParentMenu + "", ViewDataId = "" + item1.FormatCode + "", MainType = "" + item1.MainType + "", SubType = "" + item1.SubType + "", Module = "" + item1.ModuleName + "", TableName = "" + item1.TableName + "", optiontype = "" + item1.OptionType.Trim() + "", optioncode = "" + item1.OptionCode.Trim() + "", Header = "" + item1.Menu.Trim() + "", Controller2 = "" + item1.Controller2.Trim() + "" })"><li style="padding-bottom:5px;">@item1.Menu</li></a>
                                        }
                                    }
                                </ul>
                            </li>
                        }
                    }

                }
            </ul>
        </div>
    </div>
}

