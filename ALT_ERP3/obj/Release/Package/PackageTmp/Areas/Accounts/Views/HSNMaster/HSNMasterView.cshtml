@{
    ViewBag.Title = "HSN Master";
    Layout = null;
}

<script type="text/javascript">
    $(document).ready(function () {
        

        GetMyFavorites('@Model.ViewDataId');
        @* iX9: Load List for Autocomplete Inputs i.e. ControlType='A' *@
        LoadGrp();
        LoadUnit();
        LoadIGSTIn();
        LoadIGSTOut();
        LoadSGSTIn();
        LoadSGSTOut();
        LoadCGSTIn();
        LoadCGSTOut();
        LoadCessIn();
        LoadCessOut();
            
        @* iX9: Validation rules for the fields where AllowBlank is False in Definition *@
        $('#HSNMaster').validate({
            onfocusout: false,
            errorLabelContainer: $('#ErrorMessage'),
            wrapper: "li",
            rules: {
                //HSNMaster_Code: {required: true,},
                HSNMaster_Name: {required: true,},
                HSNMaster_Grp: {required: true,},
                HSNMaster_Unit: {required: true,},
               },
            messages: {
                //HSNMaster_Code: {required: 'Code is Required',},
                HSNMaster_Name: {required: 'Name is Required',},
                HSNMaster_Grp: {required: 'Grp is Required',},
                HSNMaster_Unit: {required: 'Unit is Required',},
            }
        });

        @* iX9: Input Contraints to limit the inputs to required Input Type i.e. String, Numeric etc. *@
        $("#HSNMaster_Code").constrain({ allow: { regex: "[a-zA-Z0-9()#!%$*,.\/:-+<>; ]" } });
        $("#HSNMaster_Name").constrain({ allow: { regex: "[a-zA-Z0-9()#!%$*,.\/:-+<>; ]" } });
        $("#HSNMaster_Grp").constrain({ allow: { regex: "[0-9.-]" } });
        $("#HSNMaster_Unit").constrain({ allow: { regex: "[a-zA-Z0-9()#!%$*,.\/:-+<>; ]" } });

        @* iX9: Executed when Save/Submit button is Pressed *@
        $('#Submit').click(function (event) {
            event.preventDefault();
            //if ($("#HSNMaster").validate().form() == false)
            //    return;

            if ($("#HSNMaster_Code").val() == null || $("#HSNMaster_Code").val() == '')
            {
                $("#ErrorMessage").show();
                $("#ErrorMessage").text('Code is Required..');
                return;
            }
            if ($("#HSNMaster_Name").val() == null || $("#HSNMaster_Name").val() == '')
            {
                $("#ErrorMessage").show();
                $("#ErrorMessage").text('Name is Required..');
                return;
            }
            if ($("#HSNMaster_Grp").val() == null || $("#HSNMaster_Grp").val() == 0)
            {
                $("#ErrorMessage").show();
                $("#ErrorMessage").text('Category is Required..');
                return;
            }
            if ($("#HSNMaster_Unit").val() == null || $("#HSNMaster_Unit").val() == '')
            {
                $("#ErrorMessage").show();
                $("#ErrorMessage").text('Unit is Required..');
                return;
            }
            var url = '@Url.Action("SaveData")';
            @* iX9: Data from individual field is passed on to the Model and then to Controller for Saving *@
            var ModelData = {};
            ModelData["HSNMaster_Code"] = $("#HSNMaster_Code").val();
            ModelData["HSNMaster_Name"] = $("#HSNMaster_Name").val();
            ModelData["HSNMaster_Grp"] = $("#HSNMaster_Grp").val();
            ModelData["HSNMaster_Unit"] = $("#HSNMaster_Unit").val();
            ModelData["HSNMaster_IGSTIn"] = $("#HSNMaster_IGSTIn").val();
            ModelData["HSNMaster_IGSTOut"] = $("#HSNMaster_IGSTOut").val();
            ModelData["HSNMaster_SGSTIn"] = $("#HSNMaster_SGSTIn").val();
            ModelData["HSNMaster_SGSTOut"] = $("#HSNMaster_SGSTOut").val();
            ModelData["HSNMaster_CGSTIn"] = $("#HSNMaster_CGSTIn").val();
            ModelData["HSNMaster_CGSTOut"] = $("#HSNMaster_CGSTOut").val();
            ModelData["HSNMaster_CessIn"] = $("#HSNMaster_CessIn").val();
            ModelData["HSNMaster_CessOut"] = $("#HSNMaster_CessOut").val();
            ModelData["HSNMaster_Narr"] = $("#HSNMaster_Narr").val();
            ModelData["Mode"] = '@Model.Mode';
            ModelData["Header"] = '@Model.Header';
            var DTO = { mModel: ModelData };
            $.ajax({
                type: "POST",
                dataType: "json",
                data: JSON.stringify(DTO),
                cache: false,
                url: url,
                contentType: "application/json; charset=utf-8",
                beforeSend: function () {
                    $('.spinner').show();
                },
                success: function (data) {
                    if (data.Status == 'Success') {
                        $('.spinner').hide();
                        window.location.href = "/Accounts/MasterGrid/Index?ViewDataId=@Model.ViewDataId&Module=@Model.Module&TableName=@Model.TableName&OptionType=@Model.OptionType&OptionCode=@Model.OptionCode&Header=@Model.Header&Controller2=@Model.Controller2";
                        $("#ErrorMessage").hide();
                    }
                    else if (data.Status == 'Error') {
                        alert(data.Message);
                    }
                    else {
                        $('.spinner').hide();
                        alert(data.Message);
                    }
                },
            });
        });
    });
    @* iX9: Autocomplete Code i.e. ControlType='A' and vDepend<>'' *@
    function LoadGrp() {
        var url = '@Url.Action("AutoCompleteGrp")'
        $('#HSNMaster_Grp').select2({
            minimumInputLength: 0,
            placeholder: 'Search',
            ajax: {
                url: url,
                dataType: 'json',
                quietMillis: 100,
                enableFiltering: true,
                allowClear: true,
                minimumInputLength: 0,
                multiple: true,
                width: 300,
                data: function (term, page) {
                    return {
                        types: ["exercise"],
                        limit: -1,
                        term: term
                    };
                },
                results: function (data, page) {
                    clientObj = JSON.stringify(data);
                    return {
                        results: $.map(data, function (item) {
                            return {
                                text: item.Name,
                                id: item.Code
                            }
                        })
                    };
                }
            },

            initSelection: function (element, callback) {
                var data = { id: '@Model.HSNMaster_Grp', text: '@Model.GrpName' };
                callback(data);
            },

            formatResult: function (exercise) {
                return "<div class='row-fluid'><div class=''><span class='span1'><div style='color:" + exercise.alias + "'>" + exercise.text +
                    "</div></span></div></div>";
            },

            formatSelection: function (exercise) {
                return "<div class='row-fluid'><div class=''><span class='span1'><div style='color:" + exercise.alias + "'>" + exercise.text +
                    "</div></span></div></div>";
            },
        })
    };

