        $('#btnSaveData').click(function (event) {
            event.preventDefault();


            if ($('#Account').val() == '' || $('#Account').val() == null) {
                alert('Select Account')
                return;
            }
            //if ($('#LocationCode').val() == '' || $('#LocationCode').val() == null) {
            //    alert('Select LocationCode')
            //    return;
            //}
            if ($('#Amount').val() == '0') {
