            }
            if ($("input[name=AmtType]:checked").val() == '' || $("input[name=AmtType]:checked").val() == null) {
                alert('Select Debit Or Credit')
                return;
            }
            if ($("#hdnBaseGR").val() != "D" && $("#hdnBaseGR").val() != "S" && $("#hdnBaseGR").val() != "U") {
                var tabccsize = $('#CCCentre tbody tr').length;
                if (tabccsize < 1) {
