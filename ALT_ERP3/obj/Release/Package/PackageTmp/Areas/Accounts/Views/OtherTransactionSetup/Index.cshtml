@model ALT_ERP3.Areas.Accounts.Models.OTransSetup

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_LayoutMenu.cshtml";
}
<script src="@Url.Content("~/Content/js/jquery.validate.min.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Content/js/jquery.constrain.js")" type="text/javascript"></script>
<style>
    hr {
        margin-top: 0%;
        margin-bottom: 15px;
        border: 0;
        border-top: 2px solid #e0cbcb !important;
    }

    fieldset {
        border: 1px solid #ddd !important;
        margin: 0;
        /*xmin-width: 0;*/
        padding: 10px;
        position: relative;
        border-radius: 4px;
        background-color: #f5f5f5;
        padding-left: 5px !important;
    }

    legend {
        margin-left: 1%;
        font-size: 14px;
        font-weight: bold;
        margin-bottom: 0px;
        border: 1px solid #ddd;
        border-radius: 15px;
        padding: 5px 45px 5px 45px;
        background-color: white;
        width: auto;
        color: red;
    }
</style>
<header class="header fixed-top" id="headerdiv" style="position:fixed;background-color:lightgray;padding:6px;width:100%;margin-top:0%;">
    <div class="fixhead">
        <div class="col-md-3" style="padding-left:6px;padding-top:3px;">
            <b class="CodeSt" id="headercaption">@Model.Header</b>
        </div>
        <div class="col-md-1 pull-right" style="padding-left:6px;font-family:Tahoma;padding-top:2px;padding-right:0px;font-size:9pt; width:unset;">
            <a href="~/DashBoard/SetUP" class="btn btn-danger" style="color:white;font-weight:bolder;font-size:13px;padding:3px 10px;" id="Cancel2">X</a>
        </div>
        <div class="col-md-1 pull-right" style="padding-left:6px;font-family:Tahoma;padding-top:2px;padding-right:0px;font-size:14pt;font-weight:bold;width:unset;">
            <label id="headercaption">Setup Mode</label>
        </div>
        <button class="fa fa-heart pull-right" type="submit" onclick="AddToFavorite('@Model.ViewDataId','@Model.OptionCode')" title="Add/Remove from My Favorites" id="MyFavorites" style="color: rgb(145,142,142); background-color: transparent; border: none; font-size: 20px; height: 27px; margin-right: 5px;margin-top:5px;" name="MyFavorites"></button>

    </div>
</header>
<section class="panel" style="margin-bottom:0px;">
    <div class="panel-body">
        <div class="form-horizontal " id="LCSetupMaster" role="form" style="margin-top:30px;">
            <div id="ErrorMessage" name="ErrorMessage" class="alert alert-danger" style="display: none"></div>
            <fieldset class=" col-sm-12"  style="margin-top:0%;">
                <legend>@Model.Header</legend>
                <div class="panel panel-default" style="height:430px;overflow:auto;">
                    <div class="panel-body" style="padding-top: 5px;">
                        <div class="container-fluid" style="padding-left:0px!important;padding-right:0px!important;">
                            <div class="tab">
                                <button class="tablinks" onclick="openCity(event, 'divLedgerTable')" id="List"> Basic Info</button>
                                <button class="tablinks" onclick="openCity(event, 'divLedgerTable2')" style="width:140px;"> Restriction Info</button>
                                <button class="tablinks" onclick="openCity(event, 'divLedgerTable1')" style="width:140px;"> Consignment Info</button>
                            </div>
                            <div id="divLedgerTable" class="tabcontent" style="min-height: 380px;max-height: 380px;overflow-y:auto;overflow-x:hidden;  border: 1px solid;">
                                <div class="form-group">
                                    <label class=" col-sm-2 control-label" style="font-weight: bold;" title=" How To Generate Serial No In System.">System Generate No<span style="color:red;padding-left: 5px;">*</span> :</label>
                                    <div class="col-sm-4">
                                        <label class=" col-sm-3 control-label" style="font-weight:100;" title="Branch-Wise Generate Serial No Per Year In System.">Branch Wise:</label>
                                        <div class="col-sm-1">
                                            @Html.CheckBoxFor(x => x.Class_BranchwiseSrlReq, new { @class = "", @Value = Model.Class_BranchwiseSrlReq, style = "height: 20px;width: 35px;", @title = "Branch-Wise Generate Serial No Per Year In System." })
                                        </div>
                                        <label class=" col-sm-3 control-label" style="font-weight:100;" title="Year-Wise Generate Serial No Per Year In System.">Year Wise:</label>
                                        <div class="col-sm-1">
                                            @Html.CheckBoxFor(x => x.Class_YearwiseSrlReq, new { @class = "", @Value = Model.Class_YearwiseSrlReq, style = "height: 20px;width: 35px;", @title = "Year-Wise Generate Serial No Per Year In System." })
                                        </div>
                                        <label class=" col-sm-2 control-label" style="font-weight:100;" title="Cetralised Generate Serial No In System.">Centralised:</label>
                                        <div class="col-sm-2">
                                            @Html.CheckBoxFor(x => x.Class_CetralisedSrlReq, new { @class = "", @Value = Model.Class_CetralisedSrlReq, style = "height: 20px;width: 35px;", @title = "Cetralised Generate Serial No In System." })
                                        </div>
                                    </div>
                                    <label class=" col-sm-2 control-label" style="font-weight: bold;" title="If Data Not Found In System Then System Generate Start From No. Or Also BranchWise And Year Wise Process Repeated.">Start From <span style="color:red;padding-left: 5px;">*</span> :</label>
                                    <div class="col-sm-1">
                                        @Html.TextBoxFor(x => x.Class_Srl, new { @class = "form-control", @Value = Model.Class_Srl, @title = "If Data Not Found In System Then System Generate Start From No. Or Also BranchWise And Year Wise Process Repeated." })
                                    </div>
                                    <label class=" col-sm-2 control-label" style="font-weight: bold;" title="Set Document Width.">Document Width <span style="color:red;padding-left: 5px;">*</span> :</label>
                                    <div class="col-sm-1">
                                        @Html.TextBoxFor(x => x.Class_Width, new { @class = "form-control", @Value = Model.Class_Width, @title = "Set Document Width." })
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label class=" col-sm-2 control-label" style="font-weight: bold;" title="Serial No Allow To How To Generate Auto,Manual  Or Both.">Cash/Bank Generate Mode<span style="color:red;padding-left: 5px;">*</span> :</label>
                                    <div class="col-sm-4">
                                        <label class=" col-sm-3 control-label" style="font-weight:100;">Auto:</label>
                                        <div class="col-sm-1">
                                            @Html.CheckBoxFor(x => x.BillAuto, new { @class = "", @Value = Model.BillAuto, style = "height: 20px;width: 35px;" })
                                        </div>
                                        <label class=" col-sm-3 control-label" style="font-weight:100;">Manual:</label>
                                        <div class="col-sm-1">
                                            @Html.CheckBoxFor(x => x.BillManual, new { @class = "", @Value = Model.BillManual, style = "height: 20px;width: 35px;" })
                                        </div>
                                        <label class=" col-sm-2 control-label" style="font-weight:100;">Both:</label>
                                        <div class="col-sm-2">
                                            @Html.CheckBoxFor(x => x.BillBoth, new { @class = "", @Value = Model.BillBoth, style = "height: 20px;width: 35px;" })
                                        </div>
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label class="col-sm-2 control-label" style="font-weight: bold;" title="Show Ledger Posting Of Cash/Bank Transaction.">Show Ledger Posting:</label>
                                    <div class="col-sm-2">
                                        @Html.CheckBoxFor(x => x.ShowLedgerPost, new { @class = "", @Value = Model.ShowLedgerPost, style = "height: 20px;width: 35px;", @title = "Show Ledger Posting Of Cash/Bank Transaction." })
                                    </div>
                                    <label class="col-sm-offset-2 col-sm-2 control-label" style="font-weight: bold;" title="Show Document Serial No At Saving.">Show Serial On Save:</label>
                                    <div class="col-md-4">
                                        @Html.CheckBoxFor(x => x.ShowDocSerial, new { @class = "", @Value = Model.ShowDocSerial, style = "height: 20px;width: 35px;", @title = "Show Document Serial No At Saving." })
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label class="col-sm-2 control-label" style="font-weight: bold;" title="Allow Zero Amount Of Trasaction.">Allow Zero Amount:</label>
                                    <div class="col-sm-2">
                                        @Html.CheckBoxFor(x => x.AllowZeroAmt, new { @class = "", @Value = Model.AllowZeroAmt, style = "height: 20px;width: 35px;", @title = "Allow Zero Amount Of Trasaction." })
                                    </div>
                                    <label class="col-sm-offset-2 col-sm-2 control-label" style="font-weight: bold;" title="Create Auto Remark In Cash/Bank Transaction..">Allow Auto Remark:</label>
                                    <div class="col-md-4">
                                        @Html.CheckBoxFor(x => x.AllowAutoRemark, new { @class = "", @Value = Model.AllowAutoRemark, style = "height: 20px;width: 35px;", @title = "Create Auto Remark In Cash/Bank Transaction." })
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label class="col-sm-2 control-label" style="font-weight: bold;">TDS Apply:</label>
                                    <div class="col-sm-2">
                                        @Html.CheckBoxFor(x => x.CutTDS, new { @class = "", @Value = Model.CutTDS, style = "height: 20px;width: 35px;" })
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label class="col-sm-2 control-label" style="font-weight: bold;">Delete Tyre Stock:</label>
                                    <div class="col-sm-5" style="padding-left: 0px;">
                                        <div class="col-sm-2">
                                            @Html.CheckBoxFor(x => x.TyreStock, new { @class = "", @Value = Model.TyreStock, style = "height: 20px;width: 35px;" })
                                        </div>
                                        <label class="col-sm-10 control-label" style="color:red;">Delete Document With Tyre Stock if Tyre Stock Transfer To Other Location.</label>
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label class="col-sm-3 control-label" style="font-weight: bold;" title="Particular Vehicle Not Allow Duplicate Expense In Date Range. ">Restrict Duplicate Vehicle Expense Date Range:</label>
                                    <div class="col-sm-1">
                                        @Html.RadioButtonFor(x => x.ExpDtType, "Restrict", new { @style = "padding-left:0px;width:20px;height:20px;",@title = "Particular Vehicle Not Allow Duplicate Expense In Date Range. " })
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label class="col-sm-3 control-label" style="font-weight: bold;"title = "Particular Vehicle Found Duplicate Expense In Date Range. Then Get Confirm Message In Cash/Bank Transaction. ">Confirm Duplicate Vehicle Expense Date Range:</label>
                                    <div class="col-sm-1">
                                        @Html.RadioButtonFor(x => x.ExpDtType, "Confirm", new { @style = "padding-left:0px;width:20px;height:20px;", title = "Particular Vehicle Found Duplicate Expense In Date Range. Then Get Confirm Message In Cash/Bank Transaction. " })
                                    </div>
                                </div>
                            </div>
                            <div id="divLedgerTable1" class="tabcontent" style="min-height: 380px;max-height: 380px;overflow-y:auto;overflow-x:hidden;  border: 1px solid;">
                                <div class="form-group">
                                    <label class="col-sm-3 control-label" style="font-weight: bold;">Restrict On Consignemt Date :</label>
                                    <div class="col-sm-9">
                                        <label class=" col-sm-1 control-label" style="font-weight:100;">Required :</label>
                                        <div class="col-sm-1">
                                            @Html.CheckBoxFor(x => x.RestrictLrDateExp, new { @id = "RestrictLrDateExp", @name = "RestrictLrDateExp", @class = "form-control", @Value = Model.RestrictLrDateExp, style = "height: 20px;width: 35px;" })
                                        </div>
                                        <label class="col-sm-4 control-label" style="color:red;">(Those Consignment Allows Whos Date Before </label>
                                        <div class="col-sm-1" style="padding-left: 0px;padding-right:0px;">
                                            @Html.TextBoxFor(x => x.RestrictLrExpDays, new { @class = "form-control", @Value = Model.RestrictLrExpDays, @disabled = "disabled", @maxlength = "3" })
                                        </div>
                                        <label class="col-sm-4 control-label" style="color:red;">Days Of The Document Date.) </label>
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label class="col-sm-3 control-label" style="font-weight: bold;">Duplicate Expense LR FM Confirm Message:</label>
                                    <div class="col-sm-9">
                                        <label class=" col-sm-1 control-label" style="font-weight:100;"></label>
                                        <div class="col-sm-1">
                                            @Html.CheckBoxFor(x => x.DuplExpLRFMConfirm, new { @id = "DuplExpLRFMConfirm", @name = "DuplExpLRFMConfirm", @class = "form-control", @Value = Model.DuplExpLRFMConfirm, style = "height: 20px;width: 35px;" })
                                        </div>
                                        <label class="col-sm-10 control-label"></label>
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label class="col-sm-3 control-label" style="font-weight: bold;">Restrict Duplicate Expense LR FM:</label>
                                    <div class="col-sm-9">
                                        <label class=" col-sm-1 control-label" style="font-weight:100;"></label>
                                        <div class="col-sm-1">
                                            @Html.CheckBoxFor(x => x.NoDuplExpLRFM, new { @id = "NoDuplExpLRFM", @name = "NoDuplExpLRFM", @class = "form-control", @Value = Model.NoDuplExpLRFM, style = "height: 20px;width: 35px;" })
                                        </div>
                                        <label class="col-sm-10 control-label"></label>
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label class="col-sm-3 control-label" style="font-weight: bold;">Show Consignment Expenses:</label>
                                    <div class="col-sm-9">
                                        <label class=" col-sm-1 control-label" style="font-weight:100;"></label>
                                        <div class="col-sm-1">
                                            @Html.CheckBoxFor(x => x.ShowConsignmentExp, new { @id = "ShowConsignmentExp", @name = "ShowConsignmentExp", @class = "form-control", @Value = Model.ShowConsignmentExp, style = "height: 20px;width: 35px;" })
                                        </div>
                                        <label class="col-sm-10 control-label"></label>
                                    </div>
                                </div>
                            </div>
                            <div id="divLedgerTable2" class="tabcontent" style="min-height: 380px;max-height: 380px;overflow-y:auto;overflow-x:hidden;  border: 1px solid;">
                                <div class="form-group">
                                    <label class="col-sm-2 control-label" style="font-weight: bold;" title="DocDate Allow Only Current Date In Cash/Bank Transaction.">Current DocDate Req Only :</label>
                                    <div class="col-sm-2">
                                        @Html.CheckBoxFor(x => x.Class_CurrDatetOnlyreq, new { @class = "", @Value = Model.Class_CurrDatetOnlyreq, style = "height: 20px;width: 35px;", @title = "DocDate Allow Only Current Date In Cash/Bank Transaction." })
                                    </div>

                                </div>
                                <div class="form-group">
                                    <label class="col-sm-2 control-label" style="font-weight: bold;">Enable BackDated :</label>
                                    <div class="col-sm-1">
                                        @Html.CheckBoxFor(x => x.BackDated, new { @id = "BackDated", @name = "BackDated", @class = "form-control", @Value = Model.BackDated, style = "height: 20px;width: 35px;" })
                                    </div>
                                    <label class="col-sm-1 control-label" >Back Days:</label>
                                    <div class="col-sm-2">
                                        @Html.TextBoxFor(x => x.BackDays, new { @id = "BackDays", @name = "BackDays", @class = "form-control", @Value = Model.BackDays })
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label class="col-sm-2 control-label" style="font-weight: bold;">Enable Forward Date :</label>
                                    <div class="col-sm-1">
                                        @Html.CheckBoxFor(x => x.ForwardDated, new { @id = "ForwardDated", @name = "ForwardDated", @class = "form-control", @Value = Model.ForwardDated, style = "height: 20px;width: 35px;" })
                                    </div>
                                    <label class="col-sm-1 control-label" >Forward Days:</label>
                                    <div class="col-sm-2">
                                        @Html.TextBoxFor(x => x.ForwardDays, new { @id = "ForwardDays", @name = "ForwardDays", @class = "form-control", @Value = Model.ForwardDays })
                                    </div>
                                </div>
                            </div>
                                <script>
                                    function openCity(evt, cityName) {
                                        var i, tabcontent, tablinks;
                                        tabcontent = document.getElementsByClassName("tabcontent");
                                        for (i = 0; i < tabcontent.length; i++) {
                                            tabcontent[i].style.display = "none";
                                        }
                                        tablinks = document.getElementsByClassName("tablinks");
                                        for (i = 0; i < tablinks.length; i++) {
                                            tablinks[i].className = tablinks[i].className.replace(" active", "");
                                        }
                                        document.getElementById(cityName).style.display = "block";
                                        evt.currentTarget.className += " active";
                                    }
                                    document.getElementById("List").click();

                                </script>
                            </div>
                        </div>
                </div>
            </fieldset>
        </div>
    </div>
</section>

<footer id="fixedbottom" class="footer-section" style="height:33px;position:fixed;background-color:#4B4A4A;padding:3px;color:white;">
    <div class="row">
        <div class="col-sm-5">

        </div>
        <div class="col-sm-3" style="margin-top:4px;color:white;font-family:Verdana;font-size:11px;">
            ALT AIR-3.0, Shruham Software
        </div>
        <div class="col-sm-4">
            <div class="pull-right">


                <button class="btn btn-success" style="color:black;font-size:13px;padding:3px 12px;" id="SaveData">Save</button>

                <a href="~/DashBoard/SetUP" class="btn btn-danger" style="color:black;margin-right:10px;font-size:13px;padding:3px 12px;" id="Cancel">Cancel</a>
            </div>
        </div>
    </div>
</footer>
@Html.HiddenFor(x => x.Mode, new { @id = "hdnMode" })

<script type="text/javascript">

    $(document).ready(function ()
    {
        $("#RestrictLrExpDays").constrain({ allow: { regex: "[0-9]" } });
        GetMyFavorites('@Model.ViewDataId');
        $("#Class_Width,#Class_Srl,#BackDays,#ForwardDays").constrain({ allow: { regex: "[0-9]" } });


        if ('@Model.RestrictLrDateExp' == "true" || '@Model.RestrictLrDateExp' == 'True') {
            $("#RestrictLrExpDays").attr("disabled", false);
        }
        $("#RestrictLrDateExp").change(function () {
            if (this.checked) {
                $("#RestrictLrExpDays").removeAttr("disabled");
            } else {
                $("#RestrictLrExpDays").attr("disabled", "disabled");
                $("#RestrictLrExpDays").val("0");
            }
        });
        $("#BillAuto").click(function (event) {
            if ($("#BillAuto").is(':checked')) {
                $("#BillBoth").prop('checked', false)
                $("#BillManual").prop('checked', false)
            }

        });
        $("#BillManual").click(function (event) {
            if ($("#BillManual").is(':checked')) {
                $("#BillAuto").prop('checked', false)
                $("#BillBoth").prop('checked', false)
            }

        });
        $("#BillBoth").click(function (event) {
            if ($("#BillBoth").is(':checked')) {
                $("#BillAuto").prop('checked', false)
                $("#BillManual").prop('checked', false)
            }
        });
        $('#Class_BranchwiseSrlReq').change(function () {
            if (this.checked) {
                $("#Class_YearwiseSrlReq").prop('checked', false)
                $("#Class_CetralisedSrlReq").prop('checked', false)
            }
        });
        $('#Class_YearwiseSrlReq').change(function () {
            if (this.checked) {
                $("#Class_BranchwiseSrlReq").prop('checked', false)
                $("#Class_CetralisedSrlReq").prop('checked', false)
            }
        });
        $('#Class_CetralisedSrlReq').change(function () {
            if (this.checked) {
                $("#Class_BranchwiseSrlReq").prop('checked', false)
                $("#Class_YearwiseSrlReq").prop('checked', false)
            }
        });
        $("#NoDuplExpDt").click(function (event) {
            if ($("#NoDuplExpDt").is(':checked')) {

                $("#DuplExpDtConfirm").prop('checked', false)
            }
        })
        $("#DuplExpDtConfirm").click(function (event) {
            if ($("#DuplExpDtConfirm").is(':checked')) {

                $("#NoDuplExpDt").prop('checked', false)
            }
        })

        $('#SaveData').click(function (event) {
            event.preventDefault();
            if (($("#Class_BranchwiseSrlReq").is(":checked") == false) && ($("#Class_YearwiseSrlReq").is(":checked") == false) && ($("#Class_CetralisedSrlReq").is(":checked") == false)) {
                $("#ErrorMessage").show();
                $("#ErrorMessage").text('Please Select System Generate No...!');
                return;
            }
            if ($("#Class_Srl").val() == null || $("#Class_Srl").val() == "0" || $("#Class_Srl").val() == "") {
                $("#ErrorMessage").show();
                $("#ErrorMessage").text('Please Enter Start From ...!');
                return;
            }
            if ($("#Class_Width").val() == null || $("#Class_Width").val() == "0" || $("#Class_Width").val() == "") {
                $("#ErrorMessage").show();
                $("#ErrorMessage").text('Please Enter Document Width ...!');
                return;
            } else {
                if (parseInt($("#Class_Width").val()) > 16) {
                    $("#ErrorMessage").show();
                    $("#ErrorMessage").text('Document Width Not Allow Greater Than 15 ...!');
                    return;
                }
            }
            if (($("#BillAuto").is(":checked") == false) && ($("#BillManual").is(":checked") == false) && ($("#BillBoth").is(":checked") == false)) {
                $("#ErrorMessage").show();
                $("#ErrorMessage").text('Please Select Generate Mode');
                return;
            }
            var url = '@Url.Action("SaveData")';
            @* iX9: Data from individual field is passed on to the Model and then to Controller for Saving *@
            var ModelData = {};
            ModelData["BillBoth"] = $("#BillBoth").is(':checked');
            ModelData["BillManual"] = $("#BillManual").is(':checked');
            ModelData["BillAuto"] = $("#BillAuto").is(':checked');
            ModelData["BackDays"] = $("#BackDays").val();
            ModelData["BackDated"] = $("#BackDated").is(':checked');
            ModelData["DuplExpLRFMConfirm"] = $("#DuplExpLRFMConfirm").is(':checked');
            ModelData["NoDuplExpLRFM"] = $("#NoDuplExpLRFM").is(':checked');
            ModelData["ShowConsignmentExp"] = $("#ShowConsignmentExp").is(':checked');
            ModelData["ShowLedgerPost"] = $("#ShowLedgerPost").is(':checked');
            ModelData["ForwardDated"] = $("#ForwardDated").is(':checked');
            ModelData["ForwardDays"] = $("#ForwardDays").val();
            ModelData["Mode"] = '@Model.Mode';
            ModelData["Header"] = '@Model.Header';
            ModelData["ShowDocSerial"] = $("#ShowDocSerial").is(':checked');
            ModelData["AllowZeroAmt"] = $("#AllowZeroAmt").is(':checked');
            ModelData["AllowAutoRemark"] = $("#AllowAutoRemark").is(':checked');
            ModelData["ExpDtType"] = $("input[name=ExpDtType]:checked").val();
            ModelData["TyreStock"] = $("#TyreStock").is(':checked');
            ModelData["CutTDS"] = $("#CutTDS").is(':checked');
            ModelData["RestrictLrExpDays"] = $("#RestrictLrExpDays").val();
            ModelData["RestrictLrDateExp"] = $("#RestrictLrDateExp").is(":checked");

            ModelData["Class_BranchwiseSrlReq"] = $("#Class_BranchwiseSrlReq").is(":checked");
            ModelData["Class_YearwiseSrlReq"] = $("#Class_YearwiseSrlReq").is(":checked");
            ModelData["Class_CetralisedSrlReq"] = $("#Class_CetralisedSrlReq").is(":checked");
            ModelData["Class_Srl"] = $("#Class_Srl").val();
            ModelData["Class_Width"] = $("#Class_Width").val();
            ModelData["Class_CurrDatetOnlyreq"] = $("#Class_CurrDatetOnlyreq").is(":checked");
            var DTO = { Model: ModelData };
            $.ajax({
                type: "POST",
                dataType: "json",
                data: JSON.stringify(DTO),
                cache: false,
                url: url,
                contentType: "application/json; charset=utf-8",
                beforeSend: function () {
                    $('.spinner').show();
                },
                success: function (data) {
                    if (data.Status == 'Success') {
                        $('.spinner').hide();
                        window.location.href = "/DashBoard/SetUP";
                        $("#ErrorMessage").hide();
                    }
                    else {
                        $('.spinner').hide();
                        alert(data.Message);
                    }
                },
            });
        });
    });
</script>