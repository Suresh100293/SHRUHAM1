
@model ALT_ERP3.Areas.Vehicles.Models.TyreStockTransferVM
@using Common;

<div class="form-group ">
    <label class="col-sm-1 control-label" style="font-weight: bold;">Serial No :</label>
    <div class="col-sm-2">
        @Html.TextBoxFor(x => x.OldStkTyreSerialNo, new { @class = "form-control", @readonly = "readonly" })
    </div>
    <label class="col-sm-1 control-label" style="font-weight: bold;">Stock Status :</label>
    <div class="col-sm-2">
        @Html.TextBoxFor(x => x.OldStkStatus, new { @class = "form-control", @readonly = "readonly" })
    </div>
    <label class="col-sm-1 control-label" style="font-weight: bold;">Stock At :</label>
    <div class="col-sm-2">
        @Html.TextBoxFor(x => x.OldStkAt, new { @class = "form-control", @readonly = "readonly" })
    </div>
    <label class="col-sm-1 control-label" style="font-weight: bold;">Place No :</label>
    <div class="col-sm-2">
        @Html.TextBoxFor(x => x.OldStkTyrePlcaseNo, new { @class = "form-control", @readonly = "readonly" })
    </div>
</div>
<div class="form-group ">
    <label class="col-sm-1 control-label" style="font-weight: bold;">Type Name :</label>
    <div class="col-sm-2">
        @Html.TextBoxFor(x => x.OldStkTypeName, new { @class = "form-control", @readonly = "readonly" })
    </div>
    <div class="col-sm-2 hidden">
        @Html.TextBoxFor(x => x.OldStkTypeNameCode, new { @class = "form-control", @readonly = "readonly" })
    </div>
    <label class="col-sm-1 control-label" style="font-weight: bold;">Cost :</label>
    <div class="col-sm-2">
        @Html.TextBoxFor(x => x.OldStkCost, new { @class = "form-control", @readonly = "readonly" })
    </div>
    <label class="col-sm-1 control-label" style="font-weight: bold;">Warranty KM :</label>
    <div class="col-sm-2">
        @Html.TextBoxFor(x => x.OldStkWarrantyKM, new { @class = "form-control", @readonly = "readonly" })
    </div>
    <label class="col-sm-1 control-label" style="font-weight: bold;">Stock Days :</label>
    <div class="col-sm-2">
        @Html.TextBoxFor(x => x.OldStkDays, new { @class = "form-control", @readonly = "readonly" })
    </div>
</div>
<div class="form-group ">

    <label class="col-sm-1 control-label" style="font-weight: bold;">Install In :</label>
    <div class="col-sm-2">
        @Html.TextBoxFor(x => x.OldStkInstallFor, new { @class = "form-control", @readonly = "readonly" })
    </div>
    <label class="col-sm-1 control-label" style="font-weight: bold;">Install Date :</label>
    <div class="col-sm-2">
        @Html.TextBoxFor(x => x.OldStkInstallDate, new { @class = "form-control", @readonly = "readonly" })
    </div>
    <label class="col-sm-1 control-label" style="font-weight: bold;">Install KM :</label>
    <div class="col-sm-2">
        @Html.TextBoxFor(x => x.OldStkInstallKM, new { @class = "form-control", @readonly = "readonly" })
    </div>
    <label class="col-sm-1 control-label" style="font-weight: bold;">Expiry Date :</label>
    <div class="col-sm-2">
        @Html.TextBoxFor(x => x.OldStkExpirtDate, new { @class = "form-control", @readonly = "readonly" })
    </div>
</div>

<div class="form-group ">

    <label class="col-sm-1 control-label" style="font-weight: bold;">Expiry KM :</label>
    <div class="col-sm-2">
        @Html.TextBoxFor(x => x.OldStkExpiryKM, new { @class = "form-control", @readonly = "readonly" })
    </div>

</div>

<div class="form-group hidden">
    <div class="col-sm-2">
        @Html.TextBoxFor(x => x.OldStkTablekey, new { @class = "form-control", @readonly = "readonly" })
    </div>
    <div class="col-sm-2">
        @Html.TextBoxFor(x => x.OldStkBranch, new { @class = "form-control", @readonly = "readonly" })
    </div>
</div>

