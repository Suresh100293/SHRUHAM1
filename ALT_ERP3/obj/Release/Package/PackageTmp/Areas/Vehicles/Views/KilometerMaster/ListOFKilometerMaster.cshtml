@model IEnumerable<ALT_ERP3.Areas.Vehicles.Models.KilometerCopyVM>
@using Common;
@{
    ViewBag.Title = "CopyPartial";
    Layout = null;
}

<div class="col-sm-12">

    <table id="Copy_table" class="detail zui-table" style="width:100%">
        <thead>
            <tr role="row" style="font-family:Tahoma;font-size:9pt;width:100%">
                <th style="width:3%;"></th>
                <th style="width:10%;">From Branch</th>
            </tr>
        </thead>
        <tbody>
            @if (Model != null)
            {
                foreach (var item in Model)
                {
                    <tr role="row" style="font-family:Tahoma;font-size:9pt;">
                        <td><center><input type="checkbox" class="checkbox" id="@item.DocumentNo" style="height:20px;width:20px;" /></center></td>
                        <td>@item.FromBranch</td>
                    </tr>
                }
            }
        </tbody>
    </table>
</div>
