                        <a onclick="SVGALL();">
                            <rect width="20" height="20" style="fill:rgb(201,201,199);stroke-width:3;stroke:rgb(0,0,0)" />
                        </a>
                    </svg>
                </div>
                <div class="col-md-1" style="font-family:Tahoma;padding-top:5px;font-size:9pt;display:flex;width: auto;padding-right: 0px;">
                    <label class="col-sm-12 control-label" style="width:auto;color:black;">Running Driver :</label>
