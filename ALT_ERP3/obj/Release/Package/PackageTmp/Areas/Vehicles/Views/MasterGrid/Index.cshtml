                        <title>Running Driver</title>
                        <a onclick="SVGRunning();">
                            <rect width="20" height="20" style="fill:rgb(163,228,180);stroke-width:3;stroke:rgb(0,0,0)" />
                        </a>
                    </svg>
                </div>



            }
            @if (Model.ViewDataId == "VehicleMaster")
            {

                <div class="col-md-1" style="font-family:Tahoma;padding-top:5px;font-size:9pt;display:flex;width: auto;padding-right: 0px;padding-left: 0px;">
                    <label class="col-sm-12 control-label" style="width:auto;color:black;">Active  Vehicle:</label>
                    <svg width="20" height="20">
                        <title>All Vehicle</title>
                        <a onclick="SVGActive();">
                            <rect width="20" height="20" style="fill:rgb(36,155,67);stroke-width:3;stroke:rgb(0,0,0)" />
                        </a>
                    </svg>
                </div>
