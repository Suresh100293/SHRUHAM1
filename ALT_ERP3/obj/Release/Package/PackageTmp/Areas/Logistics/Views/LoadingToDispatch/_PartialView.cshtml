        $("#btnAddNote").click(function (e) {
            e.preventDefault();
            $("#OnlyAlertNoteDiv").html('');
            var url = '/Logistics/AlertNote/ShoWAlertNoteList';
            var myTableArray = [];
            myTableArray.push($('#FMNO').val());
            var DTO = { Type: "FM000", TypeCode: myTableArray, DocTpe: "FM000" };
