        $('#btnTfatShare').click(function (event) {
            debugger;
            event.preventDefault();

            var ModelData = {};
            ModelData["Type"] = $("#Type").val();
            ModelData["Parentkey"] = $("#Parentkey").val();
            ModelData["Branch"] = $("#Branch").val();
            if ($("#Type").val() == "LR000")
            {
                ModelData["ConsignorSmsReq"] = $("#ConsignorSmsReq").is(":checked");
                ModelData["ConsignorName"] = $("#ConsignorName").val();
                ModelData["ConsignorMobileNo"] = $("#ConsignorMobileNo").val();
                ModelData["ConsignorEmailReq"] = $("#ConsignorEmailReq").is(":checked");
                ModelData["ConsignorEmailId"] = $("#ConsignorEmailId").val();
                ModelData["ConsignorFormat"] = $("#ConsignorFormat").val();


                ModelData["ConsignoeeSmsReq"] = $("#ConsignoeeSmsReq").is(":checked");
                ModelData["ConsignoeeName"] = $("#ConsignoeeName").val();
                ModelData["ConsignoeeMobileNo"] = $("#ConsignoeeMobileNo").val();
                ModelData["ConsignoeeEamilReq"] = $("#ConsignoeeEamilReq").is(":checked");
                ModelData["ConsignoeeEmailId"] = $("#ConsignoeeEmailId").val();
                ModelData["ConsignoeeFormat"] = $("#ConsignoeeFormat").val();

                ModelData["BillPartySmsReq"] = $("#BillPartySmsReq").is(":checked");
                ModelData["BillPartyName"] = $("#BillPartyName").val();
                ModelData["BillPartyMobileNo"] = $("#BillPartyMobileNo").val();
                ModelData["BillPartyEamilReq"] = $("#BillPartyEamilReq").is(":checked");
                ModelData["BillPartyEmailId"] = $("#BillPartyEmailId").val();
                ModelData["BillPartyFormat"] = $("#BillPartyFormat").val();

                ModelData["ExtraMobileNo"] = $("#ExtraMobileNo").val();
                ModelData["ExtraEmailId"] = $("#ExtraEmailId").val();
                ModelData["ExtraFormat"] = $("#ExtraFormat").val();

                ModelData["Header"] = $("#Header").val();

            }
            else if ($("#Type").val() == "SLR00" || $("#Type").val() == "SLW00") {

                ModelData["CustomerSmsReq"] = $("#CustomerSmsReq").is(":checked");
                ModelData["CustomerName"] = $("#CustomerName").val();
                ModelData["CustomerMobileNo"] = $("#CustomerMobileNo").val();
                ModelData["CustomerEamilReq"] = $("#CustomerEamilReq").is(":checked");
                ModelData["CustomerEmailId"] = $("#CustomerEmailId").val();
                ModelData["CustomerFormat"] = $("#CustomerFormat").val();


                ModelData["CustomerGroupSmsReq"] = $("#CustomerGroupSmsReq").is(":checked");
                ModelData["CustomerGroupName"] = $("#CustomerGroupName").val();
                ModelData["CustomerGroupMobileNo"] = $("#CustomerGroupMobileNo").val();
                ModelData["CustomerGroupEamilReq"] = $("#CustomerGroupEamilReq").is(":checked");
                ModelData["CustomerGroupEmailId"] = $("#CustomerGroupEmailId").val();
                ModelData["CustomerGroupFormat"] = $("#CustomerGroupFormat").val();


                ModelData["ExtraMobileNo"] = $("#ExtraMobileNo").val();
                ModelData["ExtraEmailId"] = $("#ExtraEmailId").val();
                ModelData["ExtraFormat"] = $("#ExtraFormat").val();
            }
            else if ($("#Type").val() == "BPM00") {

                ModelData["CreditorSmsReq"] = $("#CreditorSmsReq").is(":checked");
                ModelData["CreditorName"] = $("#CreditorName").val();
                ModelData["CreditorMobileNo"] = $("#CreditorMobileNo").val();
                ModelData["CreditorEamilReq"] = $("#CreditorEamilReq").is(":checked");
                ModelData["CreditorEmailId"] = $("#CreditorEmailId").val();
                ModelData["CreditorFormat"] = $("#CreditorFormat").val();

                ModelData["ExtraMobileNo"] = $("#ExtraMobileNo").val();
                ModelData["ExtraEmailId"] = $("#ExtraEmailId").val();
                ModelData["ExtraFormat"] = $("#ExtraFormat").val();
            }
            ModelData["Header"] = $("#Header").val();
            var DTO = { mModel: ModelData };
            var url = '@Url.Action("ShareDoc")';
            $.ajax({
                type: "POST",
                dataType: "json",
                data: JSON.stringify(DTO),
                cache: false,
                url: url,
                contentType: "application/json;charset=utf-8",
                beforesend: function () {
                    $('.spinner').show();
                },
                success: function (data) {
                    debugger;
                    $('.spinner').hide();
                    $('#ShareModal').modal('hide');
                },
                error: function () {
                    $('.spinner').hide();
                    alert("error occured while processing your request.");
                }
            });
        });
        $("#chkhide").change(function () {
