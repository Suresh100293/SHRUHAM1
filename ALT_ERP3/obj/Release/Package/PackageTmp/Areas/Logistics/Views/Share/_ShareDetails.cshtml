                                            @Html.TextBoxFor(x => x.ExtraEmailId, new { @type = "text", @class = "form-control", @Placeholder = "Email Id (Separated By Comma)", @id = "ExtraEmailId", @Name = "ExtraEmailId", @style = "font:size:13pt;font-weight:bolder;", @Value = Model.ExtraEmailId })
                                        </b>
                                    </td>
                                    @if (Model.Attachment)
                                    {
                                        <td style="width:19%;">@Html.ListBoxFor(x => x.ExtraFormatL, Model.PrintFormats, new { @class = "ExtraFormatL", @id = "ExtraFormatL" })</td>
                                    }
                                </tr>
                            }
                        </tbody>
                    }
                </table>
            </div>
        </div>
        <div class="modal-footer" style="background-color:#cccccc;height:42px;padding-bottom:5px;padding-top:5px;border-radius: 0px 0px 5px 5px;">
            <button id="btnTfatShare" class="btn btn-success" type="button">Send</button>
            <button data-dismiss="modal" class="btn btn-danger" type="button">Close</button>
        </div>
    </div>
</div>
