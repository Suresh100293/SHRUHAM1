
@model ALT_ERP3.Areas.Logistics.Models.ExternalAttachmentVM
<script type="text/javascript">
    $(document).ready(function ()
    {
        $("#ParentKey").val('@Model.ParentKey');
    });
</script>
<div class="form-group">
    <table class="zui-tableww" id="tblBasicDetails">
        <thead>

        </thead>
        <tbody style="background-color: burlywood !important;">

            @if (Model.Type == "LR000")
            {
                <tr>
                    <td>LRNo : @Model.Srl</td>
                    <td>From : @Model.lrmater.Source</td>
                    <td>To : @Model.lrmater.Dest</td>
                    <td>Consignor : @Model.lrmater.RecCode</td>
                    <td>Consignee : @Model.lrmater.SendCode</td>
                    <td>Qty : @Model.lrmater.TotQty</td>
                    <td>Weight : @Model.lrmater.ActWt</td>
                </tr>
            }
            else if (Model.Type == "FM000")
            {
                <tr>
                    <td>FM NO :@Model.Srl</td>
                    <td>From :@Model.FMMaster.FromBranch</td>
                    <td>Via :@Model.FMMaster.RouteViaName</td>
                    <td>To :@Model.FMMaster.ToBranch</td>
                    <td>Broker :@Model.FMMaster.BroCode</td>
                    <td>Amt :@Model.FMMaster.ChgAmt1</td>
                </tr>
            }
            else if (Model.Type == "Vehic")
            {
                <tr>
                    <td>Vehicle NO :@Model.VehicleMaster.TruckNo</td>
                    <td>Driver :@Model.VehicleMaster.Driver</td>
                    <td>Broker :@Model.VehicleMaster.Broker</td>
                </tr>
            }
            else if (Model.Type == "Drive")
            {
                <tr>
                    <td>Driver :@Model.DriverMaster.Name</td>
                    <td>Vehicle No :@Model.DriverMaster.VehicleNo</td>
                </tr>
            }
            else if (Model.Type == "SLR00" || Model.Type == "SLW00" || Model.Type == "CMM00")
            {
                <tr>
                    <td>Date :@Model.sales.BillDate.Value.ToShortDateString()</td>
                    <td>Branch :@Model.sales.Branch</td>
                    @if (Model.Type == "CMM00")
                    {
                        <td>Bank :@Model.sales.Code</td>
                    }
                    else
                    {
                        <td>Customer :@Model.sales.Code</td>
                    }
                    <td>Amt :@Model.sales.Amt</td>
                    <td>ENTEREDBY :@Model.sales.ENTEREDBY</td>
                </tr>
            }
            else if (Model.Type == "Maste")
            {
                <tr>
                    <td>Type :@Model.Master.Code</td>
                    <td>Name :@Model.Master.Name</td>
                </tr>
            }
            else if (Model.Type == "CUSMA")
            {
                <tr>
                    <td>Customer Name :@Model.customerMaster.Name</td>
                </tr>
            }
            else if (Model.Type == "PUR00")
            {
                <tr>
                    <td>Date :@Model.purchase.BillDate.Value.ToShortDateString()</td>
                    <td>Branch :@Model.purchase.Branch</td>
                    <td>Bill Number :@Model.purchase.BillNumber</td>
                    <td>Vendor Name :@Model.purchase.Code</td>
                    <td>Amt :@Model.purchase.Amt</td>
                    <td>ENTEREDBY :@Model.purchase.ENTEREDBY</td>
                </tr>
            }
            else if (Model.Type == "BLSMT")
            {
                <tr>
                    <td>Date :@Model.billSubmission.DocDate.ToShortDateString()</td>
                    <td>Branch :@Model.billSubmission.Branch</td>
                    <td>Doc Number :@Model.billSubmission.DocNo</td>
                    <td>Customer Name :@Model.billSubmission.Party</td>
                    <td>ENTEREDBY :@Model.billSubmission.ENTEREDBY</td>
                </tr>
            }
            else if (Model.Type == "CPO00" || Model.Type == "COT00")
            {
                <tr>
                    <td>Date :@Model.relateData.DocDate.Value.ToShortDateString()</td>
                    <td>Branch :@Model.relateData.Branch</td>
                    <td>Name :@Model.relateData.Code</td>
                    <td>Amt :@Model.relateData.Amount</td>
                    <td>ENTEREDBY :@Model.relateData.ENTEREDBY</td>
                </tr>
            }
            else if (Model.Type == "PCK00")
            {
                <tr>
                    <td>Date :@Model.pickOrder.BookDate.Value.ToShortDateString()</td>
                    <td>From : @Model.pickOrder.Source</td>
                    <td>To : @Model.pickOrder.Dest</td>
                    <td>Consignor : @Model.pickOrder.RecCode</td>
                    <td>Consignee : @Model.pickOrder.SendCode</td>
                    <td>Qty : @Model.pickOrder.TotQty</td>
                    <td>Bal Qty : @Model.pickOrder.BalQty</td>
                </tr>
            }

        </tbody>
    </table>
</div>
