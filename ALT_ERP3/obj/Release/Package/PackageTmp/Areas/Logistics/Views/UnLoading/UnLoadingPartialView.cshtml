    function LoadFieledSetQty() {
        

        var TotalQty = 0, TotalWeight = 0, loadQty = 0, LoadWeight = 0;

        $("#UnloadBoby .Ordertable-row").each(function () {

            var Unloadqty = $(this).find(".LQty").val();
            loadQty += parseFloat(Unloadqty);

            var Unloadweight = $(this).find(".UnWeight").text();
            LoadWeight += parseFloat(Unloadweight);

            var Qty = $(this).find(".Qty").text();
            TotalQty += parseFloat(Qty);

            var Weight = $(this).find(".Weight").text();
            TotalWeight += parseFloat(Weight);
        });


        $("#CQty").val(TotalQty);
        $("#CWght").val(TotalWeight);

        $("#LQty").val(loadQty);
        $("#LWght").val(LoadWeight);

        var BalQty = parseFloat(TotalQty) - parseFloat(loadQty);
        var BalWeight = parseFloat(TotalWeight) - parseFloat(LoadWeight);
        $("#BQty").val(BalQty);
        $("#BWght").val(BalWeight);
    }
