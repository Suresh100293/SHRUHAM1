        $('#Submit').click(function (event) {
            event.preventDefault();

            var url = '@Url.Action("SaveData")';
            @* iX9: Data from individual field is passed on to the Model and then to Controller for Saving *@
            var ModelData = {};
            ModelData["Document"] = $("#Document").val();

            //SaveLcCombo
            var DeliveryList = "", LrList = "", qtyList = "", WhatWeDo = "";
            $("#UnloadBoby .Ordertable-row").each(function () {
                if ($(this).find('input[type=checkbox]').prop("checked") == true) {
                    DeliveryList += $(this).find(".Delivery").text() + ",";
                    LrList += $(this).find(".Recorkey").text() + ",";
                    qtyList += $(this).find(".LGQty").val() + "&" + $(this).find(".LDQty").val() + "^";
                }
            });
            ModelData["ScriptDeliverylist"] = DeliveryList.substr(0, DeliveryList.length - 1)
            ModelData["ScriptLRlist"] = LrList.substr(0, LrList.length - 1)
            ModelData["ScriptQtylist"] = qtyList.substr(0, qtyList.length - 1)
            ModelData["WhatWeDo"] = WhatWeDo.substr(0, WhatWeDo.length - 1)

            var DTO = { mModel: ModelData };
            $.ajax({
                type: "POST",
                dataType: "json",
                data: JSON.stringify(DTO),
                cache: false,
                url: url,
                contentType: "application/json; charset=utf-8",
