<script>
    $(document).ready(function () {
        GetMyFavorites('@Model.ViewDataId');
        LoadBranch();
        if ('@Model.Mode' == "Delete" || '@Model.Mode' == "View") {
            $('.form-group :input').prop("disabled", true);
        }
        $("#GlobalSearchBill").constrain({ allow: { regex: "[a-zA-Z0-9]" } });
        $('#PartyComboBill').modal({
            backdrop: 'static',
            keyboard: false
        });
        $("#PartyComboBill").modal("hide");
        $("#PartyComboBill").css("display", "none");

        $('#DocDate').datepicker({
            format: 'dd-mm-yyyy',
            autoclose: true,
        });
        $('#Submit').click(function () {
            $('#ErrorMessage').hide();

            if ($("#FromBranch").val() == '' || $("#FromBranch").val() == null) {
                alert('From Branch Required...');
                return;
            }

            if ($("#tabledata tbody tr").length==0) {
                alert('Bill Required...');
                return;
            }

            var ModelData = {};

            ModelData["Document"] = $("#Document").val();
            ModelData["DocumentNo"] = $("#DocumentNo").val();
            ModelData["DocDate"] = $("#DocDate").val();
            ModelData["FromBranch"] = $("#FromBranch").val();
            ModelData["BillType"] = "Received";
            ModelData["Remark"] = $("#Remark").val();
            ModelData["Mode"] = '@Model.Mode';
            ModelData["Header"] = '@Model.Header';

            var DTO = { mModel: ModelData };
            var url = '@Url.Action("SendReceiveSaveData")';
            $.ajax({
                type: "POST",
                dataType: "json",
                data: JSON.stringify(DTO),
                cache: false,
                url: url,
                contentType: "application/json; charset=utf-8",
                beforeSend: function () {
                    $('.spinner').show();
                },
                success: function (data) {
                    $('.spinner').hide();
                    $("#ErrorMessage").hide();
                    if (data.Status == 'Success') {

                        window.location.href = "/Logistics/BillSubmission/Index?ViewDataId=@Model.ViewDataId&Module=@Model.Module&TableName=@Model.TableName&OptionType=@Model.OptionType&OptionCode=@Model.OptionCode&Header=@Model.Header&Controller2=@Model.Controller2&MainType=@Model.MainType";

                    }
                    else {
                        alert(data.Message);
                    }
                },
            });
        });

        $('#btnDelete').click(function () {
            var result = confirm("Are you sure you want to delete?");
            if (result) {
                var ModelData = {};
                ModelData["Document"] = $("#Document").val();
                ModelData["Mode"] = '@Model.Mode';
                ModelData["Header"] = '@Model.Header';
                var DTO = { mModel: ModelData };
                var url = '@Url.Action("SendReceiveSaveData")';
                $.ajax({
                    type: "POST",
                    dataType: "json",
                    data: JSON.stringify(DTO),
                    cache: false,
                    url: url,
                    contentType: "application/json; charset=utf-8",
                    beforeSend: function () {
                        $('.spinner').show();
                    },
                    success: function (data) {
                        $('.spinner').hide();
                        $("#ErrorMessage").hide();
                        if (data.Status == 'Success') {

                            window.location.href = "/Logistics/BillSubmission/Index?ViewDataId=@Model.ViewDataId&Module=@Model.Module&TableName=@Model.TableName&OptionType=@Model.OptionType&OptionCode=@Model.OptionCode&Header=@Model.Header&Controller2=@Model.Controller2&MainType=@Model.MainType";
                        }
                        else {
                            alert(data.Message);
                        }
                    },
                });
            }
        });
        $('#GlobalSearchBill').blur(function () {
            event.preventDefault();
            $('#ErrorMessage').hide();
            if ($('#GlobalSearchBill').val() != "" && $('#GlobalSearchBill').val() != null) {

                var BillDetail = {
                    BillNo: $('#GlobalSearchBill').val()
                }
                var ModelData = {};
                ModelData["BillDetail"] = BillDetail;
                ModelData["Methods"] = '@Model.Methods';
                var DTO = { Model: ModelData };
                var url = '@Url.Action("AnySearchBillNo")';
                $.ajax({
                    type: "POST",
                    dataType: "json",
                    data: JSON.stringify(DTO),
                    cache: false,
                    url: url,
                    contentType: "application/json;charset=utf-8",
                    beforeSend: function () {
                        $('.spinner').show();
                    },
                    success: function (data) {
                        $("#ErrorMessage").hide();
                        $('.spinner').hide();
                        if (data.Status == "Success") {
                            if (data.SearchCombo == "Y") {
                                $('#SearchPartyComboBill').modal('show');
                                var LoadDownLoadFiles = $("#SearchPartyComboBillList");
                                LoadDownLoadFiles.html('');
                                LoadDownLoadFiles.html(data.Html);
                            }
                            else {
                                $('#addnewbill').modal('show');
                                var LoadDownLoadFiles = $("#AddnNewBillList");
                                LoadDownLoadFiles.html('');
                                LoadDownLoadFiles.html(data.Html);
                            }

                        }
                        else if (data.Status == "Optional") {
                            var result = confirm(data.Message);
                            if (result) {
                                if (data.SearchCombo == "Y") {
                                    $('#SearchPartyComboBill').modal('show');
                                    var LoadDownLoadFiles = $("#SearchPartyComboBillList");
                                    LoadDownLoadFiles.html('');
                                    LoadDownLoadFiles.html(data.Html);
                                }
                                else {
                                    $('#addnewbill').modal('show');
                                    var LoadDownLoadFiles = $("#AddnNewBillList");
                                    LoadDownLoadFiles.html('');
                                    LoadDownLoadFiles.html(data.Html);
                                }
                            }
                            else {
                                $('#GlobalSearchBill').val('');
                            }
                        }
                        else {
                            $('#GlobalSearchBill').val('');
                            alert(data.Message);
                        }
                    },
                    error: function () {
                        alert("Error occured while processing your request.");
                    }
                });
            }
        });
        $("#DeselectAll").change(function () {
            var CheckBox = $("#DeselectAll").prop("checked");
            if (CheckBox) {
                $("#example1 > tbody  > tr").each(function () {
                    var Row = $(this).closest('tr');
                    Row.find("input[class='PODDebitGrid']").prop("checked", true);
                });
            }
            else {
                $("#example1 > tbody  > tr").each(function () {
                    var Row = $(this).closest('tr');
                    Row.find("input[class='PODDebitGrid']").prop("checked", false);
                });
            }
            debugger;
            $("#TotalBill").html($('#example1').find("input[class='PODDebitGrid']:checked").length);

        });
        $("#SelectLR").blur(function () {
            var WantToselectLRNO = $("#SelectLR").val();
            var ScrollRow;
            var count = 0;
            if ($("#SelectLR").val() != "" && $("#SelectLR").val() != null) {
                $('#example1 tbody tr').each(function () {
                    var Row = $(this).closest('tr');
                    var LRNO = Row.find('td:eq(4)').text();
                    var CheckBox = $(this).find("input[class='PODDebitGrid']").is(':checked');
                    if (LRNO == WantToselectLRNO) {
                        ScrollRow = Row;
                        Row.find("input[class='PODDebitGrid']").prop("checked", true);

                        var objTable = $("#example1");
                        var iRow = ScrollRow.find("td:eq(2)").text();
                        objTable.jqGrid('setSelection', iRow);
                        count++;
                    }
                });

                var check = $('#example1').find("input[class='PODDebitGrid']:checked").length;
                $("#TotalBill").html(check);
                debugger;
                if (count == 0) {
                    alert("Not Found");
                    $("#Search").focus();
                }
                else {
                    $("#SelectLR").focus();
                }
            }
            $("#SelectLR").val("");
        });
        // Search all columns
        $('#Search').keyup(function () {
            var value = $(this).val();
            if (value != "") {
                $('#example1 td').removeClass('SearchRow');
                $('#example1 td').removeClass('otherBranch');
                $('#example1 td').removeClass('ClickrowColor');
                $("#example1 > tbody > tr > td ").each(function () {
                    if ($(this).text().toLowerCase().indexOf(value.toLowerCase()) >= 0) {
                        var dd = $(this).text().toLowerCase();
                        $(this).addClass("SearchRow");
                    }
                    else {
                        $(this).removeClass("SearchRow");
                    }
                });
            }
            else {
                $('#example1 td').removeClass('SearchRow');
                $('#example1 td').removeClass('otherBranch');
                var check = $('#example1').find('input[type=checkbox]:checked').length;
                if (check == 0) {
                    $('#example1 td').removeClass('ClickrowColor');
                }
            }
        });
        $("#btnGetBillList").click(function (event) {
            var ModelData = {};
            var BillDetails = [];
            var BillDetails1 = [];
            $("#example1 > tbody  > tr").each(function () {
                if ($(this).find("input[class='PODDebitGrid']").prop("checked") == true) {
                    var myDets = {
                        BillNo: $(this).find('td:eq(8)').text(),
                        BillTableKey: $(this).find('td:eq(4)').text(),
                    }
                    BillDetails.push(myDets);
                    BillDetails1.push($(this).find('td:eq(8)').text());
                }
             });
             if (BillDetails.length==0) {
                 alert('Select Altealst One Bill...');
                 return;
             }


            ModelData["BillDetails"] = BillDetails;
             ModelData["Party"] = $("#Party").val();
            ModelData["FromBranch"] = $("#FromBranch").val();
             ModelData["Methods"] = '@Model.Methods';
            var DTO = { Model: ModelData };
            var url = '@Url.Action("AddBillToList")';
            $.ajax({
                type: "POST",
                dataType: "json",
                data: JSON.stringify(DTO),
                cache: false,
                url: url,
                contentType: "application/json;charset=utf-8",
                beforeSend: function () {
                    $('.spinner').show();
                },
                success: function (data) {
                    $('.spinner').hide();
                    var BillListDiv = $("#BillListDiv");
                    BillListDiv.html('');
                    BillListDiv.html(data.Html);
                    $("#PartyComboBill").modal("hide");
                    $("#PartyComboBill").css("display", "none");
                    AlertNoteMaster(BillDetails1);
                    //$('#BillListDiv').html(data.Html);
                },
                error: function (err) {
                    alert(JSON.stringify(err))
                    alert("Error occured while processing your request.");
                }
            });
        });
    });
    function LoadBranch() {
        var url = '@Url.Action("Getbranch")';
        $('#FromBranch').select2({
            minimumInputLength: 0,
            placeholder: 'Search',
            ajax: {
                url: url,
                dataType: 'json',
                quietMillis: 100,
                enableFiltering: true,
                allowClear: true,
                minimumInputLength: 0,
                multiple: true,
                width: 300,
                data: function (term, page) {
                    return {
                        types: ["exercise"],
                        limit: -1,
                        term: term
                    };
                },
                results: function (data, page) {
                    clientObj = JSON.stringify(data);
                    return {
                        results: $.map(data, function (item) {
                            return {
                                text: item.Name,
                                id: item.Code
                            }
                        })
                    };
                }
            },
            initSelection: function (element, callback) {
                var data = { id: '@Model.FromBranch', text: '@Model.FromBranchName' };
                callback(data);
            },
            formatResult: function (exercise) {
                return "<div class='row-fluid'><div class=''><span class='span1'><div style='color:" + exercise.alias + "'>" + exercise.text +
                    "</div></span></div></div>";
            },
            formatSelection: function (exercise) {
                return "<div class='row-fluid'><div class=''><span class='span1'><div style='color:" + exercise.alias + "'>" + exercise.text +
                    "</div></span></div></div>";
            },
        })

    };
    function PendingGridReload() {


        if ($("#FromBranch").val() == '' || $("#FromBranch").val() == null) {
            alert('Select FromBranch');
            return;
        }
        $('#example1').jqGrid('clearGridData');
        $("#example1").jqGrid("GridUnload");
        $('#example1').trigger('reloadGrid');

        var ColName, Coldata;
        var lPath = window.location.pathname;
        var arrPath = lPath.split("/");
        var code = $("#btncode").val();

        var sPathUrl = '@Url.Action("GetGridStructureRecords")?id=PendingBrachBillSubmission';
        var sUrlpath = '@Url.Action("GetGridData")?ViewDataId=PendingBrachBillSubmission&Code=' + $("#FromBranch").val();
        $.ajax({
            url: sPathUrl,
            type: "POST",
            contentType: "application/json; charset=utf-8",
            dataType: "json",
            beforeSend: function () {
                $('.spinner').show();
            },
            success: function (data) {
                var obj = $.parseJSON(data);
                ColName = obj[0].split(",");
                Coldata = obj[1];
                CreateGridData();
                $('.spinner').hide();
            },
            error: function () {
                alert("Error with AJAX callback");
            }
        });

        function CreateGridData() {
            $('#example1').jqGrid({
                url: sUrlpath,
                datatype: 'json',
                mtype: 'POST',
                sortable: true,
                colNames: ColName,
                colModel: Coldata,
                pager: $('#example1pager'),
                rowNum: @Session["GridRows"],
                rowList: [10, 15, 20, 50, 100, 500, 1000, 5000, 10000],
                height:350,
                sortname: '',
                frozen: false,
                sortorder: 'asc',
                gridview: true,
                viewrecords: true,
                //scroll: false,
                autowidth: false,
                shrinkToFit: false,
                search: true,
                scrollrows: true,
                onSelectRow: function (id, status, e) {
                    $('.spinner').hide();
                },
                ondblClickRow: function (id) {
                },

                beforeRequest: function () {
                    responsive_jqgridData($(".jqGrid"));
                },
                gridComplete: function () {
                    var ids = jQuery("#example1").jqGrid('getDataIDs');
                    for (var i = 0; i < ids.length; i++) {
                        var rowId = ids[i];
                        var gridId = jQuery("#example1").getDataIDs();
                        //loop through grid rows
                        var beChecked = "<input type ='checkbox' id='" + rowId + "' value='" + jQuery("#list").getCell(rowId, "Document") + "'  onclick='JQGridCheckbox();' class='PODDebitGrid' name='PODDebitGrid'  style='height: 22px;width: 19px;' />";
                        jQuery("#example1").jqGrid('setRowData', rowId, { 'Select': beChecked });
                    }
                },
            });
            jQuery("#example1").jqGrid('navGrid', "#example1pager",
            { edit: false, add: false, del: false, print:false, search: true, refresh: false, searchtext: "filter" });
            jQuery("#example1").jqGrid("setFrozenColumns");
        };

        function responsive_jqgridData(jqgrid) {
            jqgrid.find('.ui-jqgrid').addClass('clear-margin span12').css('width', '');
            jqgrid.find('.ui-jqgrid-view').addClass('clear-margin span12').css('width', '');
            jqgrid.find('.ui-jqgrid-view > div').eq(1).addClass('clear-margin span12').css('width', '').css('min-height', '0').css('display', 'grid');
            jqgrid.find('.ui-jqgrid-view > div').eq(2).addClass('clear-margin span12').css('width', '').css('min-height', '0').css('display', 'grid');
            jqgrid.find('.ui-jqgrid-sdiv').addClass('clear-margin span12').css('width', '');
            jqgrid.find('.ui-jqgrid-pager').addClass('clear-margin span12').css('width', '');
            jqgrid.find('#pager_right').css('width', '180px');
            jqgrid.find('.ui-state-default,.ui-widget-content .ui-state-default,.ui-widget-header .ui-state-default').css('background', '#e7e4e4');
            jqgrid.find('.ui-widget-content .ui-state-focus, .ui-widget-header .ui-state-focus .ui-state-hover').css('background', 'skyblue');
            $('#PartyComboBill').modal('show');
        };
    };
    function JQGridCheckbox() {

        var check = $('#example1').find("input[class='PODDebitGrid']:checked").length;
        $("#TotalBill").html(check);

    }

</script>
