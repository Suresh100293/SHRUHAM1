

@model ALT_ERP3.Areas.Logistics.Models.AlertNoteVM

    <table class="col-sm-offset-1 zui-tableww">
        <thead>
        </thead>
        <tbody style="background-color: burlywood !important;">

            @if (Model.AType == "LR000" && Model.lRMaster != null)
            {
                <tr>
                    <td>LRNo : @Model.TypeCode</td>
                    <td>From : @Model.lRMaster.Source</td>
                    <td>To : @Model.lRMaster.Dest</td>
                    <td>Consignor : @Model.lRMaster.RecCode</td>
                    <td>Consignee : @Model.lRMaster.SendCode</td>
                    <td>Qty : @Model.lRMaster.TotQty</td>
                    <td>Weight : @Model.lRMaster.ActWt</td>
                </tr>
            }
            else if (Model.AType == "FM000" && Model.fMMaster != null)
            {
                <tr>
                    <td>FM NO :@Model.TypeCode</td>
                    <td>From :@Model.fMMaster.FromBranch</td>
                    <td>Via :@Model.fMMaster.RouteViaName</td>
                    <td>To :@Model.fMMaster.ToBranch</td>
                    <td>Broker :@Model.fMMaster.BroCode</td>
                    <td>Amt :@Model.fMMaster.ChgAmt1</td>
                </tr>
            }
            else if (Model.AType == "FMH00" && Model.fMMaster != null)
            {
                <tr>
                    <td>FM NO :@Model.TypeCode</td>
                    <td>From :@Model.fMMaster.FromBranch</td>
                    <td>Via :@Model.fMMaster.RouteViaName</td>
                    <td>To :@Model.fMMaster.ToBranch</td>
                    <td>Broker :@Model.fMMaster.BroCode</td>
                    <td>Amt :@Model.fMMaster.ChgAmt1</td>
                </tr>
            }
            else if (Model.AType == "LC000" && Model.lCMaster != null)
            {
                <tr>
                    <td>Date :@Model.lCMaster.Date.ToShortDateString()</td>
                    <td>Branch :@Model.lCMaster.Branch</td>
                    <td>LC NO :@Model.lCMaster.LCno.ToString()</td>
                    <td>From :@Model.lCMaster.FromBranch</td>
                    <td>To :@Model.lCMaster.ToBranch</td>
                </tr>
            }
            else if ((Model.AType == "SLR00" || Model.AType == "SLW00") && Model.sales != null)
            {
                <tr>
                    <td>Date :@Model.sales.BillDate.Value.ToShortDateString()</td>
                    <td>Branch :@Model.sales.Branch</td>
                    <td>Customer :@Model.sales.Code</td>
                    <td>Amt :@Model.sales.Amt</td>
                    <td>ENTEREDBY :@Model.sales.ENTEREDBY</td>
                </tr>
            }
            else if (Model.AType == "CMM00" && Model.sales != null)
            {
                <tr>
                    <td>Date :@Model.sales.BillDate.Value.ToShortDateString()</td>
                    <td>Branch :@Model.sales.Branch</td>
                    <td>Bank :@Model.sales.Code</td>
                    <td>Amt :@Model.sales.Amt</td>
                    <td>ENTEREDBY :@Model.sales.ENTEREDBY</td>
                </tr>
            }
            else if (Model.AType == "PUR00" && Model.purchase != null)
            {
                <tr>
                    <td>Date :@Model.purchase.BillDate.Value.ToShortDateString()</td>
                    <td>Branch :@Model.purchase.Branch</td>
                    <td>Broker :@Model.purchase.Code</td>
                    <td>Amt :@Model.purchase.Amt</td>
                    <td>ENTEREDBY :@Model.purchase.ENTEREDBY</td>
                </tr>
            }
        </tbody>
    </table>

