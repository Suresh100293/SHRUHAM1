@model IEnumerable<ALT_ERP3.Areas.Logistics.Models.FmDetailsVM>
@using Common;
@{
    ViewBag.Title = "FM Master";
    Layout = null;

}

<script src="@Url.Content("~/Content/js/jquery.validate.min.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Content/js/jquery.constrain.js")" type="text/javascript"></script>

<script type="text/javascript">
    $(document).ready(function () {

    });
</script>

<div class="form-group">

    @if (Model != null || Model.Count() > 0)
    {
        foreach (var item in Model)
        {
            <table class="zui-table" style="width:100%">
                <thead>
                    <tr>
                        <th style="background: linear-gradient(white,#a2676c)!important;">@item.Branch</th>
                    </tr>
                </thead>
                <tbody>
                    <tr>
                        <td>
                            <table class="zui-table" style="width:100%;">
                                <thead>
                                    @foreach (var arrival in item.ArrivalDetails)
                                    {
                                        <tr>
                                            <th style="width:25%;">Arrival:- @arrival.Area</th>
                                            <th style="width:25%;">Date:- @arrival.Date</th>
                                            <th style="width:25%;">KM:- @arrival.KM</th>
                                            <th style="width:25%;">Remark:- @arrival.Remark</th>
                                        </tr>
                                    }
                                </thead>
                                
                            </table>
                        </td>
                    </tr>
                    <tr>
                        <td>
                            <table class="zui-table" style="width:100%;">
                                <thead>
                                    @foreach (var dispatch in item.DispatchDetails)
                                    {
                                        <tr>
                                            <th style="width:25%;">Dispatch:- @dispatch.Area</th>
                                            <th style="width:25%;">Date:- @dispatch.Date</th>
                                            <th style="width:25%;">KM:- @dispatch.KM</th>
                                            <th style="width:25%;">Remark:- @dispatch.Remark</th>
                                        </tr>
                                    }
                                </thead>
                                
                            </table>
                            
                        </td>
                    </tr>
                    <tr>
                        <td>
                            <table class="zui-table" style="width:100%;">
                                <thead>
                                    <tr>
                                        <th style="width:50%;">Loading</th>
                                        <th style="width:50%;">UnLoading</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <tr>
                                        <td>
                                            @foreach (var loading in item.LoadingDetails)
                                            {
                                                <br />
                                                <table class="zui-table" style="width:100%;">
                                                    <thead>
                                                        <tr>
                                                            <th>LCNo</th>
                                                            <th>Date</th>
                                                            <th>From</th>
                                                            <th>To</th>
                                                            <th>Qty</th>
                                                            <th>Weight</th>
                                                        </tr>
                                                    </thead>
                                                    <tbody>
                                                        <tr>
                                                            <td>@loading.LcNo</td>
                                                            <td>@loading.Date</td>
                                                            <td>@loading.From</td>
                                                            <td>@loading.TO</td>
                                                            <td>@loading.Qty</td>
                                                            <td>@loading.Weight</td>
                                                        </tr>
                                                    </tbody>
                                                </table>
                                                <table class="zui-table" style="width:100%;">
                                                    <thead>
                                                        <tr>
                                                            <th>Sr No</th>
                                                            <th>LRNO</th>
                                                            <th>From</th>
                                                            <th>To</th>
                                                            <th>Qty</th>
                                                            <th>Weight</th>
                                                        </tr>
                                                    </thead>
                                                    <tbody>
                                                        @foreach (var lorryRe in loading.LorryReVMs)
                                                        {
                                                            <tr>
                                                                <td>@lorryRe.Srno</td>
                                                                <td>@lorryRe.LRNo</td>
                                                                <td>@lorryRe.From</td>
                                                                <td>@lorryRe.To</td>
                                                                <td>@lorryRe.Qty</td>
                                                                <td>@lorryRe.Weight</td>
                                                            </tr>
                                                        }
                                                    </tbody>
                                                </table>
                                            }
                                        </td>
                                        <td>
                                            @foreach (var unloading in item.UnLoadingDetails)
                                            {
                                                <table class="zui-table" style="width:100%;">
                                                    <thead>
                                                        <tr>
                                                            <th>LCNo</th>
                                                            <th>Date</th>
                                                            <th>From</th>
                                                            <th>To</th>
                                                            <th>Qty</th>
                                                            <th>Weight</th>
                                                        </tr>
                                                    </thead>
                                                    <tbody>
                                                        <tr>
                                                            <td>@unloading.LcNo</td>
                                                            <td>@unloading.Date</td>
                                                            <td>@unloading.From</td>
                                                            <td>@unloading.TO</td>
                                                            <td>@unloading.Qty</td>
                                                            <td>@unloading.Weight</td>
                                                        </tr>
                                                    </tbody>
                                                </table>
                                                <table class="zui-table" style="width:100%;">
                                                    <thead>
                                                        <tr>
                                                            <th>Sr No</th>
                                                            <th>LRNO</th>
                                                            <th>From</th>
                                                            <th>To</th>
                                                            <th>Qty</th>
                                                            <th>Weight</th>
                                                        </tr>
                                                    </thead>
                                                    <tbody>
                                                        @foreach (var lorryRe in unloading.LorryReVMs)
                                                        {
                                                            <tr>
                                                                <td>@lorryRe.Srno</td>
                                                                <td>@lorryRe.LRNo</td>
                                                                <td>@lorryRe.From</td>
                                                                <td>@lorryRe.To</td>
                                                                <td>@lorryRe.Qty</td>
                                                                <td>@lorryRe.Weight</td>
                                                            </tr>
                                                        }
                                                    </tbody>
                                                </table>
                                            }
                                        </td>
                                    </tr>
                                </tbody>
                            </table>
                        </td>
                    </tr>
                </tbody>
            </table>
        }
    }


</div>
