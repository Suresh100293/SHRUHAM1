}
@if (Model.CS)
{
    if (Model.StockDetails != null)
    {
        if (Model.StockDetails.Count() > 0)
        {
            //Lorry Receipt Stock Details
            <div class="panel panel-default">
                <div class="panel-heading MyPanelHeading" id="SD">Stock Details</div>
                <div class="panel-body MyPanelBody" id="SDBody">
                    @foreach (var item in Model.StockDetails)
                    {
                        <div class="form-group">
                            <label class="col-sm-3  control-label">Branch : <spam class="SpamDataDetails">@item.StkBranch</spam></label>
                            @if (item.Type == "LR")
                            {
                                <label class="col-sm-3  control-label">Stock At : <spam class="SpamDataDetails">Godown</spam></label>
                            }
                            else
                            {
                                <label class="col-sm-3  control-label">Stock At : <spam class="SpamDataDetails">Transit</spam></label>
                            }
                            <label class="col-sm-3  control-label">Qty : <spam class="SpamDataDetails">@item.Qty</spam></label>
                        </div>
                        if (item.Type != "LR")
                        {
                            <div class="form-group">
                                <label class="col-sm-3  control-label">FM No : <spam class="SpamDataDetails">@item.FMNO</spam></label>
                                <label class="col-sm-3  control-label">
                                    Vehicle No : <spam class="SpamDataDetails">@item.VehicleNo</spam>
                                    &nbsp;&nbsp;&nbsp;<button type="button" id="btnVehicleTracking" onclick="TrackVehicle('@item.Tablekey');" value="@item.VehicleNo" class="btn btn-primary" tooltip="Tracking" title="Tracking" style="padding: 4px 12px!important;border-radius: 15px;">
                                        Track Vehicle
                                    </button>
                                </label>
                            </div>
                        }
                        <hr style="border:#eb7b7b  solid 1px" />
                    }
                </div>
            </div>
        }
        else
        {
            <div class="panel panel-default">
                <div class="panel-heading MyPanelHeading" id="SD">Stock Details</div>
                <div class="panel-body MyPanelBody" id="SDBody">
                    <div class="form-group">
                        <label class="col-sm-3  control-label" style="font-size: 20px;color: blue;">Nill Stock...</label>
                    </div>
                    <hr style="border:#eb7b7b  solid 1px" />
                </div>
            </div>
        }
    }
    else
    {
        <div class="panel panel-default">
            <div class="panel-heading MyPanelHeading" id="SD">Stock Details</div>
            <div class="panel-body MyPanelBody" id="SDBody">
                <div class="form-group">
                    <label class="col-sm-3  control-label">Nill Stock...</label>
                </div>
                <hr style="border:#eb7b7b  solid 1px" />
            </div>
        </div>
    }
}
