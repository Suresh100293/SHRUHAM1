@if (Model.PD)
{
    if (Model.PODDetails != null)
    {
        if (Model.PODDetails.Count() > 0)
        {
            //POD Details
            <div class="panel panel-default">
                <div class="panel-heading MyPanelHeading" id="PD">POD Details</div>
                <div class="panel-body MyPanelBody" id="PDBody">
                    @foreach (var item in Model.PODDetails.OrderBy(x => x.PODDate))
                    {
                        <div class="form-group">
                            <label class="col-sm-3 control-label">Branch : <spam class="SpamDataDetails">@item.PODBranch</spam></label>
                            <label class="col-sm-3 control-label">POD No : <spam class="SpamDataDetails">@item.PODno</spam></label>
                            <label class="col-sm-3 control-label">Date & Time : <spam class="SpamDataDetails">@item.PODDate.ToShortDateString() : @item.PODTime</spam></label>
                            <label class="col-sm-3 control-label">Type-Task : <spam class="SpamDataDetails">@item.PODMOduleName - @item.PODTask</spam></label>
                        </div>
                        <div class="form-group">
                            @if (item.PODTask == "Branch" && item.PODMOduleName == "POD Received")
                            {
                                <label class="col-sm-3 control-label">Received From : <spam class="SpamDataDetails">@item.PODReceiverdFromBranch</spam></label>
                            }
                            else
                            {
                                if (item.PODMOduleName != "POD Received" && item.PODTask == "Branch")
                                {
                                    <label class="col-sm-3 control-label">Send To : <spam class="SpamDataDetails">@item.PODSendToBranch</spam></label>
                                }
                            }
                            <label class="col-sm-3 control-label">Create On : <spam class="SpamDataDetails">@item.PODCreateDate.ToShortDateString()</spam></label>
                            <label class="col-sm-3 control-label">LastUpDt : <spam class="SpamDataDetails">@item.PODLastUpdateDate.ToShortDateString()</spam></label>
