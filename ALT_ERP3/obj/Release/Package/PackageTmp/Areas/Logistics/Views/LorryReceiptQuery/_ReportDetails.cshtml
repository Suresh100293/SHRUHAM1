@model ALT_ERP3.Areas.Logistics.Models.LorryReceiptQueryVM
@using Common;
@{
    /**/

    ViewBag.Title = "Index";
    Layout = "";
}
<style>
    .MyPanelHeading {
        background-color: #b9fb87 !important;
        border-color: #DDDDDD !important;
        color: #2b29db !important;
        padding: 5px 10px !important;
        border-radius: 15px !important;
        font-weight: bold !important;
    }

    .MyPanelBody {
        border: 1px solid #85cf9f !important;
        border-radius: 15px 15px 0px 0px !important;
    }

    .SpamDataDetails {
        font-size: 12px !important;
        font-weight: bold !important;
    }

    hr {
        margin: 1px 1px;
    }
</style>

<script type="text/javascript">
    $(document).ready(function () {
        $("#NSD").click(function () {
            $("#NSDBody").slideToggle("slow");
        });
        $("#CD").click(function () {
            $("#CDBody").slideToggle("slow");
        });
        $("#SD").click(function () {
            $("#SDBody").slideToggle("slow");
        });
        $("#DD").click(function () {
            $("#DDBody").slideToggle("slow");
        });
        $("#DL").click(function () {
            $("#DLBody").slideToggle("slow");
        });
        $("#DED").click(function () {
            $("#DEDBody").slideToggle("slow");
        });
        $("#PD").click(function () {
            $("#PDBody").slideToggle("slow");
        });
        $("#BD").click(function () {
            $("#BDBody").slideToggle("slow");
        });
        $("#MD").click(function () {
            $("#MDBody").slideToggle("slow");
        });
        $("#ED").click(function () {
            $("#EDBody").slideToggle("slow");
        });
    });
</script>
@if (Model.NotShowDetails)
{
    <div class="panel panel-default">
        <div class="panel-heading MyPanelHeading" id="NSD">Warning</div>
        <div class="panel-body MyPanelBody" id="NSDBody">
            <div class="form-group">
                <label class="col-sm-12  control-label" style="color:red !important;"><spam class="SpamDataDetails"> User Does Not Have Rights To See Consignment Details. </spam></label>
            </div>
        </div>
    </div>
}
@if (Model.CD)
{
    if (!String.IsNullOrEmpty(Model.LrCollection))
    {
        //Lorry Receipt Details
        <div class="panel panel-default">
            <div class="panel-heading MyPanelHeading" id="CD">Consignment Details [ @Model.Lrno ]</div>
            <div class="panel-body MyPanelBody" id="CDBody">
                <div class="form-group">
                    <label class="col-sm-3  control-label">Branch : <spam class="SpamDataDetails">@Model.LRBranch</spam></label>
                    <label class="col-sm-3  control-label">Date & Time : <spam class="SpamDataDetails">@Model.LrDate.ToShortDateString() : @Model.LrTime</spam></label>
                    <label class="col-sm-3  control-label">From : <spam class="SpamDataDetails">@Model.LrFrom</spam></label>
                    <label class="col-sm-3  control-label">To : <spam class="SpamDataDetails">@Model.LrTo</spam></label>
                </div>
                <div class="form-group">
                    <label class="col-sm-3  control-label">Consignor : <spam class="SpamDataDetails">@Model.LrConsignor</spam></label>
                    <label class="col-sm-3  control-label">Consignee : <spam class="SpamDataDetails">@Model.LrConsignee</spam></label>
                    <label class="col-sm-4  control-label">BillParty : <spam class="SpamDataDetails">@Model.LrBillParty</spam></label>
                </div>
                <div class="form-group">
                    <label class="col-sm-3  control-label">LR-Type : <spam class="SpamDataDetails">@Model.LrType</spam></label>
                    <label class="col-sm-3  control-label">BillBranch : <spam class="SpamDataDetails">@Model.LrBillBranch</spam></label>
                    <label class="col-sm-3  control-label">Qty : <spam class="SpamDataDetails">@Model.LrQty</spam></label>
                    <label class="col-sm-3  control-label">Unit : <spam class="SpamDataDetails">@Model.LrUnit</spam></label>
                </div>
                <div class="form-group">
                    <label class="col-sm-3  control-label">Act Wt : <spam class="SpamDataDetails">@Model.LrActWt</spam></label>
                    <label class="col-sm-3  control-label">Chrg Wt : <spam class="SpamDataDetails">@Model.LrChrgWt</spam></label>
                    <label class="col-sm-3  control-label">Charge Type : <spam class="SpamDataDetails">@Model.LrChargeType</spam></label>
                    <label class="col-sm-3  control-label">Description : <spam class="SpamDataDetails">@Model.LrDescription</spam></label>
                </div>
                <div class="form-group">
                    <label class="col-sm-3  control-label">Colln : <spam class="SpamDataDetails">@Model.LrCollection</spam></label>
                    <label class="col-sm-3  control-label">Delivery : <spam class="SpamDataDetails">@Model.LrDelivery</spam></label>
                    <label class="col-sm-3  control-label">LR-Mode : <spam class="SpamDataDetails">@Model.LrMode</spam></label>
                    <label class="col-sm-3  control-label">Service Type : <spam class="SpamDataDetails">@Model.LrServiceType</spam></label>
                </div>
                <div class="form-group">
                    <label class="col-sm-3  control-label">Party Chl : <spam class="SpamDataDetails">@Model.LrPartyChallan</spam></label>
                    <label class="col-sm-3  control-label">Party Inv : <spam class="SpamDataDetails">@Model.LrPartyInvoice</spam></label>
                    <label class="col-sm-3  control-label">PO-Num : <spam class="SpamDataDetails">@Model.LrPONumber</spam></label>
                    <label class="col-sm-3  control-label">BE-Num : <spam class="SpamDataDetails">@Model.LrBENumber</spam></label>
                </div>
                <div class="form-group">
                    <label class="col-sm-3  control-label">Decl Val : <spam class="SpamDataDetails">@Model.LrDeclareValue</spam></label>
                    <label class="col-sm-3  control-label">EwayBill : <spam class="SpamDataDetails">@Model.LrEwayBill</spam></label>
                    <label class="col-sm-3  control-label">GST : <spam class="SpamDataDetails">@Model.LrGST</spam></label>
                    <label class="col-sm-3  control-label">Vehicle No : <spam class="SpamDataDetails">@Model.VehicleNo</spam></label>
                </div>
                <div class="form-group">
                    <label class="col-sm-3  control-label">Create On : <spam class="SpamDataDetails">@Model.LrCreateDate.ToShortDateString()</spam></label>
                    <label class="col-sm-3  control-label">LastUpDt : <spam class="SpamDataDetails">@Model.LRLastUpdateDate.ToShortDateString()</spam></label>
                    <label class="col-sm-3  control-label">EnteredBy : <spam class="SpamDataDetails">@Model.LrEnterdby</spam></label>
                </div>
                <div class="form-group">
                    <label class="col-sm-12  control-label">Remark : <spam class="SpamDataDetails">@Model.LrRemark</spam></label>
