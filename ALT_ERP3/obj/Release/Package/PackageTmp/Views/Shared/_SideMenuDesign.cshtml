    function GetURL(mdocument) {
        //var mDate = ($('#Fromdate').val() + ':' + $('#ToDate').val());
        var mURL = '@Url.Action("GetURLDocSideBar")?mdocument=' + mdocument;
        $.ajax({
            type: 'POST',
            url: mURL,
            dataType: 'json',
            data: {},
            success: function (result) {
                $('.spinner').hide();
                if (result.url != "") {
                    window.open(result.url, '_blank');
                } else {
                    alert("Error! Can't Zoom into the Transaction!\n" + result.Message);
                }
            },
            error: function (ex) {
                alert("Error! Can't Open the Transaction" + ex);
            }
        });
        return false;
    };


    function LoadVehicles() {
        //var url = '@Url.Action("GetBranch")'
        var url = '/FirstPage/GetVehicleNo';
        $('#SearchVehicle').select2({
            minimumInputLength: 0,
            placeholder: 'Search',
            ajax: {
                url: url,
                dataType: 'json',
                quietMillis: 100,
                enableFiltering: true,
                allowClear: true,
                minimumInputLength: 0,
                multiple: true,
                width: 300,
                data: function (term, page) {
                    return {
                        types: ["exercise"],
                        limit: -1,
                        term: term
                    };
                },
                results: function (data, page) {
                    clientObj = JSON.stringify(data);
                    return {
                        results: $.map(data, function (item) {
                            return {
                                text: item.Name,
                                id: item.Code
                            }
                        })
                    };
                }
            },

            initSelection: function (element, callback) {
                var data = { id: '@Model.SearchVehicle', text: '@Model.SearchVehicle' };
                callback(data);
            },

            formatResult: function (exercise) {
                return "<div class='row-fluid'><div class=''><span class='span1'><div style='color:" + exercise.alias + "'>" + exercise.text +
                    "</div></span></div></div>";
            },

            formatSelection: function (exercise) {
                return "<div class='row-fluid'><div class=''><span class='span1'><div style='color:" + exercise.alias + "'>" + exercise.text +
                    "</div></span></div></div>";
            },
        })
    };
    function Graph(Code) {
        var c = document.getElementById(Code +"HZ");
        var ctx = c.getContext("2d");
        var c1 = document.getElementById(Code +"VR");
        var ctx1 = c1.getContext("2d");
        var c2 = document.getElementById(Code +"PIE");
        var ctx2 = c2.getContext("2d");

        //var tData = $.getValues("/FirstPage/MultiBarChartDataEF");
        var sPathUrl = '@Url.Action("MultiBarChartDataEF")?CODE=' + Code;
        $.ajax({
            url: sPathUrl,
            type: "POST",
            contentType: "application/json; charset=utf-8",
            dataType: "json",
            success: function (tData) {

                var myBarChart = new Chart(ctx, {
                    type: 'horizontalBar',
                    data: tData
                });

                var myBarChart1 = new Chart(ctx1, {
                    type: 'bar',
                    data: tData
                });

                var myBarChart2 = new Chart(ctx2, {
                    type: 'pie',
                    data: tData
                });

                $('.spinner').hide();
            },
            error: function () {
                alert("Error with AJAX callback");
            }
        });
    }
    function RefreshSideBar(Code) {
            $('.spinner').show();
            var ModelData = {};
            var url = '@Url.Action("ReloadSideMenu", "FirstPage")';
            $.ajax({
                type: "POST",
                traditional: true,
                url: url,
                data: { Code: Code },
                success: function (result) {
                    var LoadDownLoadFiles = $("#SideBarDiv");
                    LoadDownLoadFiles.html('');
                    LoadDownLoadFiles.html(result.Html);
                    $('.spinner').hide();
                }
            });
    }
</script>

