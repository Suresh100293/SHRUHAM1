@model TFATERPWebApplication.Areas.Logistics.Models.ItemMasterVM
@using Common

<style>
    .panel-heading .nav {
        margin: -23px -15px -23px;
    }

    .table, th, td {
        border: 0px solid;
    }
</style>

<script type="text/javascript">
    $(document).ready(function () {
    });
</script>

<div class="form-horizontal" id="" role="form">

    <section class="panel">
        <div class="tab">
            <button class="innertablinks2" style="width:120px;" onclick="inneropenCity2(event, 'divLedgerTable6')" id="innerSales2">General</button>
            <button class="innertablinks2" style="width:120px;" onclick="inneropenCity2(event, 'divLedgerTable7')">Rate/Taxes</button>
            <button class="innertablinks2" style="width:120px;" onclick="inneropenCity2(event, 'divLedgerTable8')">Stock Levels</button>
            <button class="innertablinks2" style="width:120px;" onclick="inneropenCity2(event, 'divLedgerTable9')">More</button>
            <button class="innertablinks2" style="width:120px;" onclick="inneropenCity2(event, 'divLedgerTable10')">Batch Details</button>
            <button class="innertablinks2" style="width:120px;" onclick="inneropenCity2(event, 'divLedgerTable11')">Stock Serial</button>
            <button class="innertablinks2" style="width:120px;" onclick="inneropenCity2(event, 'divLedgerTable12')">MRP</button>
            <button class="innertablinks2" style="width:120px;" onclick="inneropenCity2(event, 'divLedgerTable13')">Qty Control</button>
        </div>
        <div id="divLedgerTable6" class="innertabcontent2" style="overflow:scroll;height:375px;">
            @Html.Partial("General", Model)
        </div>
        <div id="divLedgerTable7" class="innertabcontent2" style="overflow:scroll;height:375px;">
            @Html.Partial("RateTaxes", Model)
        </div>
        <div id="divLedgerTable8" class="innertabcontent2" style="overflow:scroll;height:375px;">
            @Html.Partial("StockLevel", Model)
        </div>
        <div id="divLedgerTable9" class="innertabcontent2" style="overflow:scroll;height:375px;">
            @Html.Partial("More", Model)
        </div>
        <div id="divLedgerTable10" class="innertabcontent2" style="overflow:scroll;height:375px;">
            @Html.Partial("BatchDetails", Model)
        </div>
        <div id="divLedgerTable11" class="innertabcontent2" style="overflow:scroll;height:375px;">
            @Html.Partial("StockSerial", Model)
        </div>
        <div id="divLedgerTable12" class="innertabcontent2" style="overflow:scroll;height:375px;">
            @Html.Partial("MRP", Model)
        </div>
        <div id="divLedgerTable13" class="innertabcontent2" style="overflow:scroll;height:375px;">
            @Html.Partial("QtyControl", Model)
        </div>
    </section>
</div>

<script>
    function inneropenCity2(evt, cityName) {
        var i, innertabcontent2, tablinks;
        innertabcontent2 = document.getElementsByClassName("innertabcontent2");
        for (i = 0; i < innertabcontent2.length; i++) {
            innertabcontent2[i].style.display = "none";
        }
        tablinks = document.getElementsByClassName("innertablinks2");
        for (i = 0; i < tablinks.length; i++) {
            tablinks[i].className = tablinks[i].className.replace("active", "");
        }
        document.getElementById(cityName).style.display = "block";
        evt.currentTarget.className += " active";
    }
    document.getElementById("innerSales2").click();
</script>
