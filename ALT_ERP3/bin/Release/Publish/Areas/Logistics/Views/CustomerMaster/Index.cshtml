            ModelData["GSTNo"] = $("#GSTNo").val();
            ModelData["CSTNo"] = $("#CSTNo").val();
            ModelData["VATReg"] = $("#VATReg").val();
            ModelData["TINNo"] = $("#TINNo").val();
            ModelData["Designation"] = $("#Designation").val();
