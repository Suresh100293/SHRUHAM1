@model TFATERPWebApplication.Areas.Logistics.Models.ArrivalDispatchVM
@{
    Layout = null;
}
<style>
    .span6 {
        display: none !important;
    }
</style>
<script type="text/javascript">
    $(document).ready(function () {
        $('#tblUnLoadingDetailstblList').DataTable();
        $('#tblUnLoadingDetailstbllcList').DataTable();

    });
</script>
@if (Model.lCModals != null)
{
    <div class="container-fluid">

        <center>
            <table class="Ordertable table-bordered table-bordered example table-responsive-md" id="tblUnLoadingDetailstbllcList" style="width:-webkit-fill-available;">
                <thead id="Head">
                    <tr>
                        <th class="Ordertable-cell">LCno</th>
                        <th class="Ordertable-cell">Date</th>
                        <th class="Ordertable-cell">Time</th>
                        <th class="Ordertable-cell">From</th>
                        <th class="Ordertable-cell">To</th>
                        <th class="Ordertable-cell">Total-Qty</th>
                        <th class="Ordertable-cell">Total-Weight</th>
                    </tr>
                </thead>
                <tbody id="LcCombo1" style="max-height:300px;min-height:300px;width:100%;overflow:scroll;">
                    @foreach (var item in Model.lCModals)
                    {
                        <tr class="Ordertable-row">
                            <td class="Ordertable-cell">@item.lcno</td>
                            <td class="Ordertable-cell">@item.Date</td>
                            <td class="Ordertable-cell">@item.Time</td>
                            <td class="Ordertable-cell">@item.From_Name</td>
                            <td class="Ordertable-cell">@item.To_Name</td>
                            <td class="Ordertable-cell">@item.TotalQty</td>
                            <td class="Ordertable-cell">@item.Weight</td>
                        </tr>
                    }
                </tbody>

            </table>
        </center>
        <br />
        <center>
            <table class="Ordertable table-bordered table-bordered example table-responsive-md" id="tblUnLoadingDetailstblList" style="width:-webkit-fill-available;">
                <thead id="Head">
                    <tr>
                        <th class="Ordertable-cell">LCno</th>
                        <th class="Ordertable-cell">LrNO</th>
                        <th class="Ordertable-cell">Date</th>
                        <th class="Ordertable-cell">Time</th>
                        <th class="Ordertable-cell">From</th>
                        <th class="Ordertable-cell">To</th>
                        <th class="Ordertable-cell">ChargeType</th>
                        <th class="Ordertable-cell">ProductType</th>
                        <th class="Ordertable-cell">Total-Qty</th>
                        <th class="Ordertable-cell">Unload-Qty</th>
                        <th class="Ordertable-cell">Balance-Qty</th>

                    </tr>
                </thead>
                <tbody id="LcCombo" style="max-height:300px;min-height:300px;width:100%;overflow:scroll;">
                    @foreach (var item1 in Model.lRMOdals)
                    {
                        <tr class="Ordertable-row">
                            <td class="Ordertable-cell">@item1.Lcno</td>
                            <td class="Ordertable-cell">@item1.Lrno</td>
                            <td class="Ordertable-cell">@item1.Date</td>
                            <td class="Ordertable-cell">@item1.Time</td>
                            <td class="Ordertable-cell">@item1.From_Name</td>
                            <td class="Ordertable-cell">@item1.To_Name</td>
                            <td class="Ordertable-cell">@item1.ChargeType</td>
                            <td class="Ordertable-cell">@item1.PorductType</td>
                            <td class="Ordertable-cell">@item1.Qty</td>
                            <td class="Ordertable-cell">@item1.unloadQty</td>
                            <td class="Ordertable-cell">@item1.AllowQty</td>
                            
                        </tr>
                    }
                </tbody>

            </table>
        </center>
    </div>
    <br />

}





