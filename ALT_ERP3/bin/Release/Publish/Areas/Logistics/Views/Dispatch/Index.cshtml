<!--Dispatch Modal -->
<div class="example-modal">
    <div id="Dispatchmodal" class="modal modal-default">
        <div class="modal-dialog modal-lg">
            @*style="width:1080px;"*@
            <div id="DispatchmodalDiv">
                <div class="modal-body">
                    <div class="modal-content">
                        <div class="modal-header" style="background-color:#cccccc;padding-top:5px;">
                            <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
                            <div style="display: inline-flex;">
                                <h4 class="modal-title col-sm-4"><b>Dispatch</b></h4>
                                <div class="col-sm-offset-1"></div>
                                <label id="ErrorMessage" name="ErrorMessage" class="alert alert-danger col" style="display: none;padding: 8px;margin-left: 25px;min-width: 400px;max-width: 400px;"></label>
                            </div>

                        </div>
                        <div class="modal-body" style="min-height:135px; max-height:135px; width:100%;">
                            @*<div id="ErrorMessage" name="ErrorMessage" class="alert alert-danger" style="display: none;margin-top:0px;"></div>*@

                            <div class="form-group col-sm-12">
                                <label class=" col-sm-1 control-label" style="margin-top: 3px;font-weight:bold;color:black;">Fm No:</label>
                                <div class="col-sm-2" style="width: 110px;">
                                    @Html.TextBoxFor(x => x.DFmNo, new { @class = "form-control", @style = "", @Value = Model.DFmNo, @readonly = "readonly" })
                                </div>
                                <label class="col-sm-1 control-label" style="width: 90px;margin-top: 3px;font-weight:bold;color:black;">Vehicle No:</label>
                                <div class="col-sm-2">
                                    @Html.TextBoxFor(x => x.DVehicleNo, new { @class = "form-control", @style = "", @Value = Model.DVehicleNo, @readonly = "readonly" })
                                </div>
                                <label class="col-sm-1 control-label" style="margin-top: 3px;font-weight:bold;color:black;">From:</label>
                                <div class="col-sm-2" style="">
                                    @Html.TextBoxFor(x => x.DFrom, new { @class = "form-control", @style = "", @Value = Model.DFrom, @readonly = "readonly" })
                                </div>
                                <label class="col-sm-1 control-label" style="margin-top: 3px;font-weight:bold;color:black;">To:</label>
                                <div class="col-sm-2" style="margin-bottom:5px;">
                                    @Html.TextBoxFor(x => x.DTo, new { @class = "form-control", @style = "", @Value = Model.DTo, @readonly = "readonly" })
                                </div>
                            </div>
                            <div class="form-group col-sm-12">
                                <label class="col-sm-1 control-label" style="margin-top: 3px;font-weight:bold;color:black;">Date:</label>
                                <div class="col-sm-1" style="margin-bottom:5px;width:110px;">
                                    <input type="text" style="" disabled class="form-control" value="@DateTime.Now.ToShortDateString() " id="DispatchDate" />
                                </div>
                                <label class="col-sm-1 control-label" style="width: 90px;margin-top: 3px;font-weight:bold;color:black;">Time:</label>
                                <div class="col-sm-2" style="margin-bottom:5px;">
                                    <input type="time" class="form-control" disabled style="" value="@DateTime.Now.ToString("hh:mm")" id="DispatchTime" />
                                </div>
                                <label class="col-sm-1 control-label" style="margin-top: 3px;font-weight:bold;color:black;">KM:</label>
                                <div class="col-sm-2" style="margin-bottom:5px;">
                                    <input type="text" class="form-control" id="DispatchKM" />
                                </div>
                            </div>
                            <div class="form-group col-sm-12">
                                <label class="col-sm-1 control-label" style="margin-top: 3px;font-weight:bold;color:black;">Remark:</label>
                                <div class="col-sm-11" style="margin-bottom:5px;">
                                    <textarea rows="1" class="form-control" maxlength="100" id="DispatchRemark" /></textarea>
                                </div>
                            </div>


                        </div>
                        <div class="modal-footer" style="background-color:#cccccc;height:42px;padding-bottom:5px;padding-top:5px;border-radius: 0px 0px 5px 5px;">
                            <button data-dismiss="modal" class="btn btn-danger" type="button">Close</button>
                            <button class="btn btn-success" type="button" id="btnSubmit">Submit</button>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

