                    <label class="col-sm-1 control-label">App.Branch:</label>
                    <div class="col-sm-3">
                        @Html.ListBoxFor(x => x.AppBranchItemX, Model.AppBranchMultiX, new { @class = "AppBranchlistbox", @id = "TfatPass_AppBranch" })
                    </div>
                </div>*@
                <div class="form-group">
                    <label class="col-sm-1 control-label">Corporate ID:</label>
                    <div class="col-sm-3">
                        @Html.TextBoxFor(x => x.TfatPass_CorpID, new { @maxlength = "10", @class = "form-control", @Name = "TfatPass_CorpID", @Value = Model.TfatPass_CorpID, @Id = "TfatPass_CorpID" })
                    </div>
