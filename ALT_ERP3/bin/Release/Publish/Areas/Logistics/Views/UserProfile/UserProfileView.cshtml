@* iX9: Form Design *@
<header class="header fixed-top" style="position:fixed;background-color:lightgray;padding:6px;width:100%;margin-top:5px;">
    <div class="fixhead">
        <b class="CodeSt">@Model.Header</b>
        <span class="tools pull-right">
            <a href="~/Logistics/MasterGrid/Index?ViewDataId=@Model.ViewDataId&Module=@Model.Module&TableName=@Model.TableName&OptionType=@Model.OptionType&OptionCode=@Model.OptionCode&Header=@Model.Header&Controller2=@Model.Controller2&MainType=@Model.MainType" class="btn btn-danger" style="color:white;font-weight:bolder;font-size:13px;padding:3px 10px;" id="Cancel2">X</a>
        </span>
        <span class="tools pull-right">
            <b class="CodeSt">@Model.Mode Mode</b>
        </span>
    </div>
</header>

<section class="panel" style="margin-bottom:0px;">
    <div class="panel-body">
        <div class="form-horizontal " id="UserProfile" role="form" style="margin-top:15px;">
            <div id="ErrorMessage" name="ErrorMessage" class="alert alert-danger" style="display: none"></div>
            <br />
            @* iX9: -- Code for Header Starts -- *@
            @* iX9: Tab Headers *@
            <div class="tab">
                <button class="tablinks" onclick="OpenTab(event, 'divGeneral')" id="FirstTab">General</button>
                <button class="tablinks" onclick="OpenTab(event, 'divLogins')">Logins</button>
                <button class="tablinks" onclick="OpenTab(event, 'divMailing')">Mailing</button>
                <button class="tablinks" onclick="OpenTab(event, 'divPreferences')">Preferences</button>
                <button class="tablinks" onclick="OpenTab(event, 'divAppBranch')">Area Access</button>
            </div>
            @* iX9: Tab Body Panels *@
            <div id="divGeneral" style="overflow:scroll;height:500px;" class="tabcontent">
                <div class="form-group">
                    <label class="col-sm-1 control-label">User ID:</label>
                    <div class="col-sm-3">
                        @if (Model.Mode != "Add")
                        {
                            @Html.TextBoxFor(x => x.TfatPass_Code, new { @maxlength = "15", @readonly = "readonly", @class = "form-control", @Name = "TfatPass_Code", @Value = Model.TfatPass_Code, @Id = "TfatPass_Code" })
                        }
                        else
                        {
                            @Html.TextBoxFor(x => x.TfatPass_Code, new { @maxlength = "15", @class = "form-control", @Name = "TfatPass_Code", @Value = Model.TfatPass_Code, @Id = "TfatPass_Code" })
                        }
                    </div>
                </div>
                <div class="form-group">
                    <label class="col-sm-1 control-label">Name:</label>
                    <div class="col-sm-3">
                        @Html.TextBoxFor(x => x.TfatPass_Name, new { @maxlength = "40", @class = "form-control", @Name = "TfatPass_Name", @Value = Model.TfatPass_Name, @Id = "TfatPass_Name" })
                    </div>
                </div>
                <div class="form-group">
                    <label class="col-sm-1 control-label">IsHead:</label>
                    <div class="col-sm-1">
                        @Html.CheckBoxFor(x => x.TfatPass_IsHead, new { @value = Model.TfatPass_IsHead, @id = "TfatPass_IsHead", @name = "TfatPass_IsHead", @style = "height:16px;width:16px;" })
                    </div>
                </div>
                <div class="form-group">
                    <label class="col-sm-1 control-label">User Role:</label>
                    <div class="col-sm-3">
                        @Html.TextBoxFor(x => x.TfatPass_UserRole, new { @class = "input-large-query", @style = "width:100%;margin-top:2px;", @id = "TfatPass_UserRole", @Value = Model.TfatPass_UserRole, @Name = "TfatPass_UserRole" })
                    </div>
                </div>
                <br />
                <div class="form-group">
                    <label class="col-sm-1 control-label">Dept:</label>
                    <div class="col-sm-3">
                        @Html.TextBoxFor(x => x.TfatPass_Dept, new { @class = "input-large-query", @style = "width:100%;margin-top:2px;", @id = "TfatPass_Dept", @Value = Model.TfatPass_Dept, @Name = "TfatPass_Dept" })
                    </div>
                </div>
                <div class="form-group">
                    <label class="col-sm-1 control-label">Language:</label>
                    <div class="col-sm-3">
                        @Html.TextBoxFor(x => x.TfatPass_Language, new { @class = "input-large-query", @style = "width:100%;margin-top:2px;", @id = "TfatPass_Language", @Value = Model.TfatPass_Language, @Name = "TfatPass_Language" })
                    </div>
                </div>
                <div class="form-group">
                    <label class="col-sm-1 control-label">Assistant:</label>
                    <div class="col-sm-3">
                        @Html.TextBoxFor(x => x.TfatPass_Assistant, new { @class = "input-large-query", @style = "width:100%;margin-top:2px;", @id = "TfatPass_Assistant", @Value = Model.TfatPass_Assistant, @Name = "TfatPass_Assistant" })
                    </div>
                </div>
                <div class="form-group" style="display:none;">
