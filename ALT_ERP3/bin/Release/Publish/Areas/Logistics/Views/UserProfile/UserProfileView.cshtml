                        @Html.TextBoxFor(x => x.TfatPass_Photograph, new { @maxlength = "150", @class = "form-control", @Name = "TfatPass_Photograph", @Value = Model.TfatPass_Photograph, @Id = "TfatPass_Photograph" })
