@model TFATERPWebApplication.Models.UserProfileVM
@using Common;
@{
    ViewBag.Title = "User Profile";
    Layout = null;
}

<link href="~/Content/bootstrap-multiselect.css" rel="stylesheet" />
<script src="~/Content/bootstrap-multiselect.js"></script>
<script src="~/Scripts/jquery.jstree.js"></script>

<script type="text/javascript">
    $(document).ready(function () {

        FillTreeView();
        if ('@Model.Mode' == "Delete") {
            $('.form-group :input').prop("disabled", true);
        }

        $('#TfatPass_Expiry').datepicker({
            format:'dd-mm-yyyy',
            autoclose: true,
        });
        @* iX9: Load List for Autocomplete Inputs i.e. ControlType='A' *@
        LoadUserRole()
        LoadDept()
        LoadLanguage()
        LoadAssistant()

        @* iX9: Multi-Select Controls *@
        //var values = $('#hdnAppBranch').val();
        //$("#hdnAppBranchArray").val(values);
        //if (values == "") {
        //}
        //else {
        //    $.each(values.split(","), function (i, e) {
        //    $(".AppBranchlistbox option[value='" + e + "']").prop("selected", true);
        //    });
        //}
        //$(".AppBranchlistbox").change(function () {
        //    $('#hdnAppBranchArray').val('');
        //    var selected = [];
        //    $(".AppBranchlistbox :selected").each(function () {
        //        selected.push($(this).val());
        //    });
        //    $('#hdnAppBranchArray').val(selected)
        //    return false;
        //});
        //$('.AppBranchlistbox').multiselect({ includeSelectAllOption: true })

        @* iX9: set the selected value to fixed combos *@
        $('#TfatPass_GridRows').val('@Model.TfatPass_GridRows').attr("selected", "selected");
        @* iX9: Date Picker Code *@
        $('#TfatPass_Expiry').datepicker({
            format: 'dd-mm-yyyy',
            autoclose: true,
        });

        @* iX9: Validation rules for the fields where AllowBlank is False in Definition *@
        $('#UserProfile').validate({
            onfocusout: false,
            errorLabelContainer: $('#ErrorMessage'),
            wrapper: "li",
            rules: {
                TfatPass_Code: {required: true,},
                TfatPass_AppBranch: {required: true,},
                TfatPass_Name: {required: true,},
               },
            messages: {
                TfatPass_Code: {required: 'Code is Required',},
                TfatPass_AppBranch: {required: 'AppBranch is Required',},
                TfatPass_Name: {required: 'Name is Required',},
            }
        });

        @* iX9: Input Contraints to limit the inputs to required Input Type i.e. String, Numeric etc. *@
        $("#TfatPass_Code").constrain({ allow: { regex: "[a-zA-Z0-9()#!%$*,.\/:-+<>; ]" } });
        $("#TfatPass_GridRows").constrain({ allow: { regex: "[0-9.-]" } });
        $("#TfatPass_Name").constrain({ allow: { regex: "[a-zA-Z0-9()#!%$*,.\/:-+<>; ]" } });
        $("#TfatPass_MinItem").constrain({ allow: { regex: "[0-9.-]" } });
        $("#TfatPass_MinAcc").constrain({ allow: { regex: "[0-9.-]" } });
        $("#TfatPass_MinOthers").constrain({ allow: { regex: "[0-9.-]" } });
        $("#TfatPass_Dept").constrain({ allow: { regex: "[0-9.-]" } });
        $("#TfatPass_Language").constrain({ allow: { regex: "[0-9.-]" } });
        $("#TfatPass_SMTPPort").constrain({ allow: { regex: "[0-9.-]" } });

        @* iX9: Executed when Save/Submit button is Pressed *@
        $('#Submit').click(function (event) {
            event.preventDefault();
            //if ($("#UserProfile").validate().form() == false)
            //    return;

            if ($("#TfatPass_Code").val() == null || $("#TfatPass_Code").val() == '')
            {
                $("#ErrorMessage").show();
                $("#ErrorMessage").text('User ID is Required..');
                return;
            }
            var BranchList = "";
            $('#treeviewcontrol').each(function () {
                $(this).find('li').each(function () {
                    var current = $(this);
                    if (current.hasClass("jstree-checked")) {
                        BranchList += current.context.id + ",";
                    }
                });
            });
            BranchList = BranchList.substr(0, BranchList.length - 1);
            if (BranchList == '' || BranchList == null) {
                $("#ErrorMessage").show();
                $("#ErrorMessage").text('App.Branch is Required..');
                return;
            }
            //if ($("#TfatPass_AppBranch").val() == null || $("#TfatPass_AppBranch").val() == '')
            //{
            //    $("#ErrorMessage").show();
            //    $("#ErrorMessage").text('App.Branch is Required..');
            //    return;
            //}
            if ($("#TfatPass_Name").val() == null || $("#TfatPass_Name").val() == '')
            {
                $("#ErrorMessage").show();
                $("#ErrorMessage").text('Name is Required..');
                return;
            }
            var url = '@Url.Action("SaveData")';
            @* iX9: Data from individual field is passed on to the Model and then to Controller for Saving *@
            var ModelData = {};
            ModelData["TfatPass_Mobile"] = $("#TfatPass_Mobile").val();
            ModelData["TfatPass_Code"] = $("#TfatPass_Code").val();
            //ModelData["TfatPass_AppBranch"] = $("#hdnAppBranchArray").val();
            ModelData["TfatPass_AppBranch"] = BranchList;
            ModelData["TfatPass_GridRows"] = $("#TfatPass_GridRows").val();
            ModelData["TfatPass_CorpID"] = $("#TfatPass_CorpID").val();
            ModelData["TfatPass_Email"] = $("#TfatPass_Email").val();
            ModelData["TfatPass_Name"] = $("#TfatPass_Name").val();
            ModelData["TfatPass_MinItem"] = $("#TfatPass_MinItem").val();
            ModelData["TfatPass_PrintControl"] = $("#TfatPass_PrintControl").is(':checked');
            ModelData["TfatPass_IsHead"] = $("#TfatPass_IsHead").is(':checked');
            ModelData["TfatPass_MinAcc"] = $("#TfatPass_MinAcc").val();
            ModelData["TfatPass_Telephone"] = $("#TfatPass_Telephone").val();
            ModelData["TfatPass_Hide"] = $("#TfatPass_Hide").is(':checked');
            ModelData["TfatPass_Locked"] = $("#TfatPass_Locked").is(':checked');
            ModelData["TfatPass_DontChangePassword"] = $("#TfatPass_DontChangePassword").is(':checked');
            ModelData["TfatPass_MinOthers"] = $("#TfatPass_MinOthers").val();
            ModelData["TfatPass_UserRole"] = $("#TfatPass_UserRole").val();
            ModelData["TfatPass_DashboardActive"] = $("#TfatPass_DashboardActive").is(':checked');
            ModelData["TfatPass_LogIns"] = $("#TfatPass_LogIns").val();
            ModelData["TfatPass_POPServer"] = $("#TfatPass_POPServer").val();
            ModelData["TfatPass_ExpiryVM"] = $("#TfatPass_Expiry").val();
            ModelData["TfatPass_PassWords"] = $("#TfatPass_PassWords").val();
            ModelData["TfatPass_SMTPServer"] = $("#TfatPass_SMTPServer").val();
            ModelData["TfatPass_Dept"] = $("#TfatPass_Dept").val();
            ModelData["TfatPass_PassRead"] = $("#TfatPass_PassRead").val();
            ModelData["TfatPass_Language"] = $("#TfatPass_Language").val();
            ModelData["TfatPass_DailyPassword"] = $("#TfatPass_DailyPassword").is(':checked');
            ModelData["TfatPass_SMTPUser"] = $("#TfatPass_SMTPUser").val();
            ModelData["TfatPass_RecMsg"] = $("#TfatPass_RecMsg").is(':checked');
            ModelData["TfatPass_Sun"] = $("#TfatPass_Sun").is(':checked');
            ModelData["TfatPass_RecSMS"] = $("#TfatPass_RecSMS").is(':checked');
            ModelData["TfatPass_SMTPPassword"] = $("#TfatPass_SMTPPassword").val();
            ModelData["TfatPass_Assistant"] = $("#TfatPass_Assistant").val();
            ModelData["TfatPass_Mon"] = $("#TfatPass_Mon").is(':checked');
            ModelData["TfatPass_Tue"] = $("#TfatPass_Tue").is(':checked');
            ModelData["TfatPass_Photograph"] = $("#TfatPass_Photograph").val();
            ModelData["TfatPass_SMTPPort"] = $("#TfatPass_SMTPPort").val();
            ModelData["TfatPass_Wed"] = $("#TfatPass_Wed").is(':checked');
            ModelData["TfatPass_Thu"] = $("#TfatPass_Thu").is(':checked');
            ModelData["TfatPass_Signature"] = $("#TfatPass_Signature").val();
            ModelData["TfatPass_Fri"] = $("#TfatPass_Fri").is(':checked');
            ModelData["TfatPass_Sat"] = $("#TfatPass_Sat").is(':checked');
            ModelData["TfatPass_StrictIP"] = $("#TfatPass_StrictIP").val();
            ModelData["Mode"] = '@Model.Mode';
            var DTO = { mModel: ModelData };
            $.ajax({
                type: "POST",
                dataType: "json",
                data: JSON.stringify(DTO),
                cache: false,
                url: url,
                contentType: "application/json; charset=utf-8",
                beforeSend: function () {
                    $('.spinner').show();
                },
                success: function (data) {
                    if (data.Status == 'Success') {
                        $('.spinner').hide();
                        window.location.href = "/Logistics/MasterGrid/Index?ViewDataId=@Model.ViewDataId&Module=@Model.Module&TableName=@Model.TableName&OptionType=@Model.OptionType&OptionCode=@Model.OptionCode&Header=@Model.Header&Controller2=@Model.Controller2&MainType=@Model.MainType";
                        $("#ErrorMessage").hide();
                    }
                    else {
                        $('.spinner').hide();
                        alert(data.Message);                    }
                },
            });
        });
    });
    @* iX9: Autocomplete Code i.e. ControlType='A' and vDepend<>'' *@
    function LoadUserRole() {
        var url = '@Url.Action("AutoCompleteUserRole")'
        $('#TfatPass_UserRole').select2({
            minimumInputLength: 0,
            placeholder: 'Search',
            ajax: {
                url: url,
                dataType: 'json',
                quietMillis: 100,
                enableFiltering: true,
                allowClear: true,
                minimumInputLength: 0,
                multiple: true,
                width: 300,
                data: function (term, page) {
                    return {
                        types: ["exercise"],
                        limit: -1,
                        term: term
                    };
                },
                results: function (data, page) {
                    clientObj = JSON.stringify(data);
                    return {
                        results: $.map(data, function (item) {
                            return {
                                text: item.Name,
                                id: item.Code
                            }
                        })
                    };
                }
            },

            initSelection: function (element, callback) {
                var data = { id: '@Model.TfatPass_UserRole', text: '@Model.UserRoleName' };
                callback(data);
            },

            formatResult: function (exercise) {
                return "<div class='row-fluid'><div class=''><span class='span1'><div style='color:" + exercise.alias + "'>" + exercise.text +
                    "</div></span></div></div>";
            },

            formatSelection: function (exercise) {
                return "<div class='row-fluid'><div class=''><span class='span1'><div style='color:" + exercise.alias + "'>" + exercise.text +
                    "</div></span></div></div>";
            },
        })
    };

