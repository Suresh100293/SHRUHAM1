                    </div>
                    <label class="col-sm-1 control-label">Readonly Password:</label>
                    <div class="col-sm-3">
                        @Html.TextBoxFor(x => x.TfatPass_PassRead, new { @maxlength = "150", @class = "form-control", @Name = "TfatPass_PassRead", @Value = Model.TfatPass_PassRead, @Id = "TfatPass_PassRead" })
                    </div>
                </div>
                <div class="form-group">
