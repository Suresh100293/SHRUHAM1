<ul id="treeview">
    <li class="parent  Company ">
        Head Office
        @if (ViewBag.treeMainMenu != null)
        {
            <ul>
                @foreach (var item in (ViewBag.treeMainMenu as IEnumerable<TFATERPWebApplication.Models.NFlatObject>).ToList())
                {
                    if (item.ParentId == "-1")
                    {
                        <li class="parent   Createzone " id="@item.Id">
                            @item.data
                            @foreach (var item1 in (ViewBag.treeMainMenu as IEnumerable<TFATERPWebApplication.Models.NFlatObject>).ToList())
                            {
                                if (item.Id == item1.ParentId)
                                {
                                    <ul>
                                        <li class="parent  CreateBranch" id="@item1.Id" ParentId="@item1.ParentId">
                                            @item1.data
                                            @foreach (var item2 in (ViewBag.treeMainMenu as IEnumerable<TFATERPWebApplication.Models.NFlatObject>).ToList())
                                            {
                                                if (item1.Id == item2.ParentId)
                                                {
                                                    <ul>
                                                        <li class="CreateArea " id="@item2.Id" ParentId="@item2.ParentId">@item2.data</li>
                                                    </ul>
                                                }

                                            }
                                        </li>
                                    </ul>
                                }
                            }
                        </li>
                    }
                }
            </ul>
        }
    </li>
</ul>
