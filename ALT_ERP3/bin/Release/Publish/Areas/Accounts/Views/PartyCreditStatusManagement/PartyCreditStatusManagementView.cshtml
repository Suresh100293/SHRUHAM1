            ModelData["HoldTransactions_CRDaysWarn"] = $("#HoldTransactions_CRDaysWarn").is(':checked');
            ModelData["HoldTransactions_HoldOrderDt2VM"] = $("#HoldTransactions_HoldOrderDt2").val();
            ModelData["HoldTransactions_HoldDespatch"] = $("#HoldTransactions_HoldDespatch").is(':checked');
            ModelData["HoldTransactions_HoldDespatchDt1VM"] = $("#HoldTransactions_HoldDespatchDt1").val();
            ModelData["HoldTransactions_ChkTempCRLimit"] = $("#HoldTransactions_ChkTempCRLimit").is(':checked');
            ModelData["HoldTransactions_TempCrLimit"] = $("#HoldTransactions_TempCrLimit").val();
            ModelData["HoldTransactions_HoldDespatchDt2VM"] = $("#HoldTransactions_HoldDespatchDt2").val();
            ModelData["HoldTransactions_TempCrLimitDt1VM"] = $("#HoldTransactions_TempCrLimitDt1").val();
