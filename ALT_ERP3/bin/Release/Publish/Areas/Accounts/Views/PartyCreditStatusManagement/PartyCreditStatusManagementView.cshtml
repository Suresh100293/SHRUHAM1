            ModelData["HoldTransactions_HoldInvoiceDt1VM"] = $("#HoldTransactions_HoldInvoiceDt1").val();
            ModelData["HoldTransactions_HoldInvoiceDt2VM"] = $("#HoldTransactions_HoldInvoiceDt2").val();
            ModelData["HoldTransactions_HoldPayment"] = $("#HoldTransactions_HoldPayment").is(':checked');
            ModelData["HoldTransactions_ChkTempCRDays"] = $("#HoldTransactions_ChkTempCRDays").is(':checked');
