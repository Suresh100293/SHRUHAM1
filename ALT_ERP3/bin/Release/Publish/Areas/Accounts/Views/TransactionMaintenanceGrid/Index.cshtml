                            }
                            if (trxstatus == '%G') {
                                $(mytrxgrid).jqGrid('setRowData', trxrows[i], false, { color: 'Black', weightfont: 'bolder', background: '#7fbf7f' });
                            }
                            if (trxstatus == '%Y') {
