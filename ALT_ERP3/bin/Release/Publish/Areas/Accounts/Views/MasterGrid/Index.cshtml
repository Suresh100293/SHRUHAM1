    @if (ViewBag.DrillQuery != "")
    {
        <div class="col-sm-9" style="padding-left:0px;padding-right:0px;">
            <div class="panel-body" style="margin-top:29px;height:auto;padding-left:0px;padding-right:0px;">
                <div id="page-contentgrid">
                    <div class="jqGrid">
                        <table id="list" class="scroll" cellpadding="0" cellspacing="0" style="border-collapse:unset;"></table>
                        <div id="pager" style="height:34px;"></div>
                    </div>
                </div>
            </div>
        </div>
        <div class="col-sm-3" style="padding-left:0px;padding-right:0px;">
            <div class="panel-body" style="margin-top:29px;height:auto;padding-left:0px;padding-right:0px;width:100%;">
                <div class="col-sm-12" style="padding-left:0px;padding-right:0px;overflow:scroll;">
                    <div id="page-contentgridright">
                        <div class="jqGrid">
                            <table id="list2" class="scroll" cellpadding="0" cellspacing="0" style="border-collapse:unset;"></table>
                            <div id="pager2" style="height:34px;"></div>
                        </div>
                    </div>
                </div>
            </div>
