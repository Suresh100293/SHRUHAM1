@model TFATERPWebApplication.Areas.Accounts.Models.PurchaseVM
@using Common;

<div class="grid" id="SelectedledgerTable" style="max-height:300px; width:100%; overflow-y: scroll;">
    <table id="tabledata" class="zui-table" style="width:100%;word-wrap:break-word">
        <thead>
            <tr role="row" style="font-family:Tahoma;font-size:9pt;width:100%;">
                <th style="width:1%">Sr.</th>
                <th style="width:10%">Item Code</th>
                <th style="width:13%;">Item Description</th>
                <th style="width:10%;">Store</th>
                <th class="Bintdcl" style="width:6%;">BIN</th>
                <th style="width:5%">Unit</th>
                <th style="width:5%">Qty</th>
                <th class="MultiUnitCl" style="width:5%">Factor</th>
                <th class="MultiUnitCl" style="width:7%">Unit2</th>
                <th class="MultiUnitCl" style="width:1%"></th>
                <th class="MultiUnitCl" style="width:5%">Qty2</th>
                <th style="width:1%"></th>
                <th style="width:5%">HSN</th>
                <th class="sgtvatlbl" style="width:7%">GST Code</th>
                <th style="width:5%">Rate</th>
                <th colspan="4" style="width:6%"></th>
            </tr>
            <tr class="2ndrow" role="row" style="font-family:Tahoma;font-size:9pt;">
                <td></td>
                <td><input type="text" id="txtitemcode" style="width:99%; margin-top:2px;height:25px;" readonly="readonly" action="validateSession" /></td>
                <td><input type="text" id="txtitemdesc" class="input-large search-query" style="width:99%;margin-top:2px;height:25px;color:black;" action="validateSession" /></td>
                <td>@Html.DropDownListFor(x => x.Store, DropdownHelper.EmptyList(), new { @class = "form-control", @id = "Store", @style = "color:black;" })</td>
                <td class="Bintdcl">@Html.DropDownListFor(x => x.BinNumber, DropdownHelper.EmptyList(), new { @class = "form-control", @id = "BinNumber", @style = "color:black;" })</td>
                <td>@Html.TextBoxFor(x => x.Unit, new { @type = "text", @id = "txtUnit", @style = "width:99%;margin-top:2px;height:25px;color:black;", @disabled = "disabled" })</td>
                <td>@Html.TextBoxFor(x => x.Qty, new { @id = "txtQty", @style = "width:99%;margin-top:2px;text-align:right;height:25px;color:black;" })</td>
                <td class="MultiUnitCl">@Html.TextBoxFor(x => x.Factor, new { @id = "txtFactor", @style = "width:99%;margin-top:2px;text-align:right;height:25px;color:black;" })</td>
                <td class="MultiUnitCl">@Html.DropDownListFor(x => x.Unit2, DropdownHelper.EmptyList(), new { @class = "form-control", @id = "txtUnit2", @style = "color:black;" })</td>
                <td class="MultiUnitCl">@Html.CheckBoxFor(x => x.RateOn2, new { @id = "RateOn2" })</td>
                <td class="MultiUnitCl">@Html.TextBoxFor(x => x.Qty2, new { @id = "txtQty2", @style = "width:99%;margin-top:2px;text-align:right;height:25px;color:black;" })</td>
                <td><button type="button" id="btnitemAddon" class="btn btn-primary btn-sm" title="Item AddOn" style="border:1px solid black"><i class="fa fa-shopping-cart" style="color:black;height:16px;"></i></button></td>
                <td>@Html.DropDownListFor(x => x.HSN, DropdownHelper.EmptyList(), new { @class = "form-control", @id = "txtHSN", @Name = "txtHSN", @style = "color:black;" })</td>
                <td>@Html.DropDownListFor(x => x.GSTCode, DropdownHelper.EmptyList(), new { @class = "form-control", @id = "txtGSTCode", @Name = "txtGSTCode", @style = "color:black;" })</td>
                <td>@Html.TextBoxFor(x => x.Rate, new { @id = "txtRate", @style = "width:99%;margin-top:2px;text-align:right;height:25px;color:black;" })</td>
                <td style="vertical-align:inherit;" colspan="2"> <button type="button" id="btnadddata" class="btn btn-primary btn-sm" title="Add" style="margin-left:10px;width:60%;height:25px;border:1px solid black"><i class="fa fa-plus" style="color:black;"></i></button> </td>
            </tr>
            <tr class="3rdrow" role="row" style="font-family:Tahoma;font-size:9pt;">
                <td style="width:1%"></td>
                <td colspan="15">
                    Disc% @Html.TextBoxFor(x => x.Disc, new { @id = "txtDisc", @style = "width:3%;margin-top:2px;text-align:right;height:25px;color:black;" })
                    @Html.TextBoxFor(x => x.DiscPerAmt, new { @id = "txtDiscPerAmt", @hidden = "hidden" })
                    Disc @Html.TextBoxFor(x => x.DiscAmt, new { @id = "txtDiscAmt", @style = "width:4%;text-align:right;margin-top:2px;height:25px;color:black;" })
                    @if (Model.SubType != "PK")
                    {
                        <button type="button" id="btndiscchargesAdd" class="btn btn-info btn-sm" title="discount charges" style="border:1px solid black;height:20px;"><i class="fa fa-keyboard-o" style="color:black"></i></button>
                    }
                    <label id="cvdext"> CVD:Extra </label>@Html.TextBoxFor(x => x.CVDExtra, new { @id = "txtCVDExtra", @style = "width:3%;margin-top:2px;text-align:right;height:25px;color:black;" })
                    <label id="cvdamt"> Amt </label> @Html.TextBoxFor(x => x.CVDAmt, new { @id = "txtCVDAmt", @style = "width:3%;margin-top:2px;text-align:right;height:25px;color:black;" })
                    <label id="cvdcess"> Cess </label>  @Html.TextBoxFor(x => x.CVDCessAmt, new { @id = "txtCVDCessAmt", @style = "width:3%;margin-top:2px;text-align:right;height:25px;color:black;" })
                    <label id="cvdscess"> SCess </label>  @Html.TextBoxFor(x => x.CVDSCessAmt, new { @id = "txtCVDSCessAmt", @style = "width:3%;margin-top:2px;text-align:right;height:25px;color:black;" })
                    Taxable <input id="txtTaxable" type="text" style="margin-top:2px;text-align:right;height:25px;width:99px;color:black;" disabled="disabled" />
                    <label class="igstlbl">IGST</label>  @Html.TextBoxFor(x => x.PIgst, new { @id = "PIgst", @style = "margin-top:2px;text-align:right;height:25px;width:30px;color:black;", @disabled = "disabled" })
                    @Html.TextBoxFor(x => x.IGSTAmt,  new
               {
                   @id = "txtIGSTAmt",
                   @style = "margin-top:2px;text-align:right;height:25px;width:60px;color:black;",
                   @disabled = "disabled"
               })
                    <label class="sgstlbl">SGST</label>
                    @Html.TextBoxFor(x => x.PSgst, new
                     {
                        @id = "PSgst",
                        @style = "margin-top:2px;text-align:right;height:25px;width:30px;color:black;",
                        @disabled = "disabled"
                     }) @Html.TextBoxFor(x => x.SGSTAmt,  new
                     {
                      @id = "txtSGSTAmt",
                      @style = "margin-top:2px;text-align:right;height:25px;width:60px;color:black;",
                      @disabled = "disabled"
                   })
                    <label class="cgstlbl">CGST</label>
                    @Html.TextBoxFor(x => x.PCgst, new
                       {
                          @id = "PCgst",
                          @style = "margin-top:2px;text-align:right;height:25px;width:30px;color:black;",
                          @disabled = "disabled"
                       }) @Html.TextBoxFor(x => x.CGSTAmt,  new
                      {
                      @id = "txtCGSTAmt",
                      @style = "margin-top:2px;text-align:right;height:25px;width:60px;color:black;",
                      @disabled = "disabled"
                     })
                    Value <input id="txtVal1" type="text" style="margin-top:2px;text-align:right;height:25px;width:80px;color:black;" />
                </td>

                <td style="vertical-align:inherit;" colspan="2">
                    <button type="button" id="btndelaysch" class="btn btn-info btn-sm" title="dely Schedule" style="border:1px solid black;"><i class="fa fa-calendar" style="color:black;width:12px;height:12px;"></i></button>
                    @if (Model.SubType == "PK")
                    {
                        <button type="button" id="btnpack" class="btn btn-success btn-sm" title="Pack List" style="border:1px solid black"><i class="fa fa-dropbox" style="color:black;width:12px;height:12px;"></i></button>
                    }
                    @if (Model.SubType == "RP" || Model.SubType == "CP" || Model.SubType == "IM" || Model.SubType == "NS" || Model.SubType == "IC" || Model.SubType == "SX" || Model.SubType == "RS" || Model.SubType == "CS" || Model.SubType == "XS" || Model.SubType == "NP" || Model.SubType == "OC" || Model.SubType == "PX" || Model.SubType == "OS")
                    {
                        <button type="button" id="btnstksrl" class="btn btn-warning btn-sm" title="Stk Serial" style="border:1px solid black"><i class="fa fa-list" style="color:black;width:12px;height:12px;"></i></button>
                        <button type="button" id="btnbatch" class="btn btn-success btn-sm" title="Item Batch" style="border:1px solid black"><i class="fa fa-file" style="color:black;width:12px;height:12px;"></i></button>
                    }
                </td>
            </tr>
            <tr role="row" style="font-family:Tahoma;font-size:9pt;">
                <td></td>
                <td colspan="15">
                    Stock @Html.TextBoxFor(x => x.Stock, new { @id = "txtStock", @style = "width:80px;text-align:right;margin-top:2px;height:25px;color:black;", @disabled = "disabled" })
                    AllStock @Html.TextBoxFor(x => x.Stock2, new { @id = "txtStock2", @style = "margin-top:2px;text-align:right;height:25px;width:80px;color:black;", @disabled = "disabled" })
                    Narration @Html.TextAreaFor(x => x.Narr, new
               {
                   @type = "text",
                   @id = "txtNarr",
                   @style = "margin-top:2px;height:25px;width:70%;margin-bottom:-7px;color:black;"
               })

                </td>
            </tr>
        </thead>
    </table>
</div>

<div class="grid" id="SelectedledgerTable" style="max-height:300px; width:100%; overflow: scroll;">
    <table id="tabledata2" class="zui-table2" style="width:100%">
        <tbody>
            @if (Model.NewItemList != null)
            {
                foreach (var item in @Model.NewItemList.Where(x => x.tempIsDeleted == false).ToList())
                {
                    <tr role="row" style="font-family:Tahoma;font-size:9pt;">
                        <td style="width:1%">@item.tempId</td>
                        <td colspan="13">
                            <table id="Inntabledata2" style="width: 101%; margin:-6px -6px -6px -6px; ">
                                <tr style="height:10px;">
                                    <td colspan="3" style="width:20%">[@item.Code]  @item.ItemName</td>
                                    <td style="width:9%">@item.StoreName</td>
                                    <td class="Bintdcl" style="width:4%">@item.BinNumber</td>
                                    <td style="width:5%">@item.Unit</td>
                                    <td style="width:5%;text-align:right;">@item.Qty</td>
                                    <td colspan="3" class="MultiUnitCl" style="width:15%;text-align:left;">Factor:@item.Factor |Qty2: @item.Qty2|Unit2: @item.Unit2|  @item.RateOn2  </td>
                                    <td style="width:5%">@item.HSN</td>
                                    <td style="width:7%">@item.GSTCode</td>
                                    <td style="width:5%;text-align:right;">@item.Rate</td>
                                </tr>
                                @if (Model.SubType == "IM")
                                {
                                    <tr>
                                        <td style="width:6%">Disc%: @GetFixedTwo(@item.Disc) Disc: @GetFixedTwo(@item.DiscAmt)</td>
                                        <td style="width:8%">CVD: Extra @item.CVDExtra|Amt @item.CVDAmt|Cess @item.CVDCessAmt|SCess @item.CVDSCessAmt</td>
                                        <td style="width:6%">Taxable: @GetFixedTwo(@item.Taxable)</td>
                                        <td colspan="4"><label class="igstlbl">IGST</label>: @GetFixedTwo(@item.IGSTRate)% @GetFixedTwo(@item.IGSTAmt) |<label class="sgstlbl">SGST</label>: @GetFixedTwo(@item.SGSTRate)% @GetFixedTwo(@item.SGSTAmt) | <label class="cgstlbl">CGST</label>: @GetFixedTwo(@item.CGSTRate)% @GetFixedTwo(@item.CGSTAmt) </td>
                                        <td style="width:4%">@item.IndKey @item.EnqKey @item.QtnKey @item.OrdKey @item.PKSKey @item.ChlnKey @item.InvKey </td>
                                        <td style="text-align:right;font-weight:bold;background-color:lightgray;" colspan="2">@GetFixedTwo(@item.Val1)</td>
                                        @*<td colspan="3">Narration: @item.Narr</td>*@
                                    </tr>
                                }
                                else
                                {
                                    <tr>
                                        <td style="width:6%">Disc%: @GetFixedTwo(@item.Disc)</td>
                                        <td style="width:6%">Disc: @GetFixedTwo(@item.DiscAmt)</td>
                                        <td style="width:6%">Taxable: @GetFixedTwo(@item.Taxable)</td>
                                        <td colspan="4"><label class="igstlbl">IGST</label>: @GetFixedTwo(@item.IGSTRate)% @GetFixedTwo(@item.IGSTAmt) |<label class="sgstlbl">SGST</label>: @GetFixedTwo(@item.SGSTRate)% @GetFixedTwo(@item.SGSTAmt) | <label class="cgstlbl">CGST</label>: @GetFixedTwo(@item.CGSTRate)% @GetFixedTwo(@item.CGSTAmt) </td>
                                        <td style="width:4%">@item.IndKey @item.EnqKey @item.QtnKey @item.OrdKey @item.PKSKey @item.ChlnKey @item.InvKey</td>
                                        <td style="text-align:right;font-weight:bold;background-color:lightgray;" colspan="2">@GetFixedTwo(@item.Val1)</td>
                                        @*<td colspan="3">Narration: @item.Narr</td>*@
                                    </tr>
                                }
                            </table>
                        </td>
                        <td colspan="2">
                            <table style="width: 101%; margin:0px 0px -6px -6px;">
                                <tr>
                                    <td style="border:none;">
                                        <button type="button" action="EditItem" class="btn btn-warning btn-sm" style="padding:0px 5px;border:1px solid black" value="@item.tempId" title="Edit"><i class="fa fa-edit" style="color:black"></i></button>
                                        <button type="button" action="DeleteItem" class="btn btn-danger btn-sm" value="@item.tempId" title="Delete" style="padding:0px 5px;border:1px solid black"><i class="fa fa-trash-o" style="color:black"></i></button>
                                    </td>
                                </tr>
                                <tr>
                                    <td colspan="2" style="border:none;">
                                        <button type="button" action="ShowDelayAddSess" class="btn btn-info btn-sm" title="dely Schedule" value="@item.tempId" style="border:1px solid black"><i class="fa fa-calendar" style="color:black"></i></button>
                                        @if (Model.SubType == "PK")
                                        {
                                            <button type="button" action="ShowPackListSess" class="btn btn-success btn-sm" title="Pack List" value="@item.tempId" style="border:1px solid black"><i class="fa fa-dropbox" style="color:black"></i></button>
                                        }
                                    </td>
                                </tr>
                            </table>
                        </td>
                    </tr>
                }
            }
        </tbody>
    </table>
</div>

